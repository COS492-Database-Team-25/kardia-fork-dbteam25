$Version=2$
a_account_category_component "widget/component-decl"
    {
    width=835;height=635;
    a_account_category_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :a_ledger_number, :a_is_interfund_xfer, :a_acct_cat_desc, :s_date_created, :a_is_intrafund_xfer, :a_acct_type, :a_account_category, :s_date_modified, :s_created_by, :__cx_osml_control, :a_acct_cat_comment FROM /lightsys/partner_DB/a_account_category/rows";
        baseobj = "/lightsys/partner_DB/a_account_category/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        a_account_category_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            a_account_category_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            a_account_category_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                a_account_category_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_account_category_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_account_category; text="account category"; type="editbox"; form=a_account_category_form; tooltip="account code (alphanumeric allowed) -- "; }
                    a_ledger_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_ledger_number; text="ledger number"; type="editbox"; form=a_account_category_form; tooltip="ledger number that uses this account -- "; }
                    a_acct_type_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_acct_type; text="acct type"; type="editbox"; form=a_account_category_form; tooltip="type of account that this category applies to: (A)sset, (L)iability, (Q)Equity, (R)evenue, (E)xpense -- "; }
                    }
                a_account_category_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_is_intrafund_xfer_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_is_intrafund_xfer; text="is intrafund xfer"; type="editbox"; form=a_account_category_form; tooltip="account is used for intra-fund transfers -- "; }
                    a_is_interfund_xfer_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_is_interfund_xfer; text="is interfund xfer"; type="editbox"; form=a_account_category_form; tooltip="account is used for inter-fund transfers -- "; }
                    a_acct_cat_desc_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_acct_cat_desc; text="acct cat desc"; type="editbox"; form=a_account_category_form; tooltip="short description of account for reporting -- "; }
                    }
                a_account_category_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_acct_cat_comment_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_acct_cat_comment; text="acct cat comment"; type="editbox"; form=a_account_category_form; tooltip="comments / long description of account -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=a_account_category_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=a_account_category_form; tooltip=" -- "; }
                    }
                a_account_category_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=a_account_category_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=a_account_category_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=a_account_category_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
