$Version=2$
e_document_component "widget/component-decl"
    {
    width=835;height=635;
    e_document_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :e_orig_filename, :e_current_filename, :s_date_created, :e_doc_type_id, :e_workflow_instance_id, :e_image_height, :s_date_modified, :e_uploading_collaborator, :s_created_by, :__cx_osml_control, :e_title, :e_image_width, :e_document_id, :e_current_folder FROM /lightsys/partner_DB/e_document/rows";
        baseobj = "/lightsys/partner_DB/e_document/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        e_document_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            e_document_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            e_document_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                e_document_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_document_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_document_id; text="document id"; type="editbox"; form=e_document_form; tooltip="The ID of the document. -- "; }
                    e_doc_type_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_doc_type_id; text="doc type id"; type="editbox"; form=e_document_form; tooltip="The ID of the document type -- "; }
                    e_title_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_title; text="title"; type="editbox"; form=e_document_form; tooltip="The title of this document. -- "; }
                    }
                e_document_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_orig_filename_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_orig_filename; text="orig filename"; type="editbox"; form=e_document_form; tooltip="The original filename of the document when it was uploaded -- "; }
                    e_current_folder_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_current_folder; text="current folder"; type="editbox"; form=e_document_form; tooltip="The pathname of the folder the document is currently stored in. -- "; }
                    e_current_filename_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_current_filename; text="current filename"; type="editbox"; form=e_document_form; tooltip="The current filename of the document, as stored. -- "; }
                    }
                e_document_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_uploading_collaborator_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_uploading_collaborator; text="uploading collaborator"; type="editbox"; form=e_document_form; tooltip="The collaborator or collaborator group that uploaded the document -- "; }
                    e_workflow_instance_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_workflow_instance_id; text="workflow instance id"; type="editbox"; form=e_document_form; tooltip="The workflow instance of this document, if any. This is used for workflow state chains triggered by the document upload itself rather than by the association of the document with a particular partner. -- "; }
                    e_image_width_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_image_width; text="image width"; type="editbox"; form=e_document_form; tooltip="The width, in pixels, of the image (if this file is am image) -- "; }
                    }
                e_document_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_image_height_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_image_height; text="image height"; type="editbox"; form=e_document_form; tooltip="The height, in pixels, of the image (if this file is am image) -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=e_document_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=e_document_form; tooltip=" -- "; }
                    }
                e_document_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=e_document_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=e_document_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=e_document_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
