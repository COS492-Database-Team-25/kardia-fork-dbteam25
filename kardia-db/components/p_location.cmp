$Version=2$
p_location_component "widget/component-decl"
    {
    width=835;height=635;
    p_location_osrc "widget/osrc"
        {
        sql="SELECT :p_postal_mode, :p_purge_date, :p_postal_code, :p_date_effective, :p_address_3, :p_address_2, :p_location_type, :p_postal_barcode, :p_record_status_code, :p_country_code, :p_partner_key, :p_address_1, :__cx_osml_control, :p_date_good_until, :p_certified_date, :p_state_province, :p_in_care_of, :s_modified_by, :p_location_comments, :s_date_created, :p_city, :s_date_modified, :p_revision_id, :p_location_id, :s_created_by, :p_bulk_postal_code, :p_postal_status FROM /lightsys/partner_DB/p_location/rows";
        baseobj = "/lightsys/partner_DB/p_location/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        p_location_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            p_location_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            p_location_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                p_location_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_partner_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_partner_key; text="partner key"; type="editbox"; form=p_location_form; tooltip=" -- "; }
                    p_location_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_location_id; text="location id"; type="editbox"; form=p_location_form; tooltip=" -- "; }
                    p_revision_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_revision_id; text="revision id"; type="editbox"; form=p_location_form; tooltip="0 if current address, or <n> if it is an older revision. -- "; }
                    }
                p_location_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_location_type_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_location_type; text="location type"; type="editbox"; form=p_location_form; tooltip="H=home, W=work, S=school, V=vacation -- "; }
                    p_date_effective_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_date_effective; text="date effective"; type="editbox"; form=p_location_form; tooltip=" -- "; }
                    p_date_good_until_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_date_good_until; text="date good until"; type="editbox"; form=p_location_form; tooltip=" -- "; }
                    }
                p_location_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_purge_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_purge_date; text="purge date"; type="editbox"; form=p_location_form; tooltip=" -- "; }
                    p_in_care_of_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_in_care_of; text="in care of"; type="editbox"; form=p_location_form; tooltip=" -- "; }
                    p_address_1_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_address_1; text="address 1"; type="editbox"; form=p_location_form; tooltip=" -- "; }
                    }
                p_location_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_address_2_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_address_2; text="address 2"; type="editbox"; form=p_location_form; tooltip=" -- "; }
                    p_address_3_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_address_3; text="address 3"; type="editbox"; form=p_location_form; tooltip=" -- "; }
                    p_city_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_city; text="city"; type="editbox"; form=p_location_form; tooltip=" -- "; }
                    }
                p_location_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_state_province_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_state_province; text="state province"; type="editbox"; form=p_location_form; tooltip="Use country-keyed ref tbl 4 state/prov/county -- "; }
                    p_country_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_country_code; text="country code"; type="editbox"; form=p_location_form; tooltip="Use ISO codes same as domain names -- "; }
                    p_postal_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_postal_code; text="postal code"; type="editbox"; form=p_location_form; tooltip=" -- "; }
                    }
                p_location_hbox5 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_postal_mode_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_postal_mode; text="postal mode"; type="editbox"; form=p_location_form; tooltip="B-Bulk F-FirstClass Used to override postal modes on mailings -- "; }
                    p_bulk_postal_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_bulk_postal_code; text="bulk postal code"; type="editbox"; form=p_location_form; tooltip="USPS - zip center 3 digits -- "; }
                    p_certified_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_certified_date; text="certified date"; type="editbox"; form=p_location_form; tooltip="e.g., CASS certification of address -- "; }
                    }
                p_location_hbox6 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_postal_status_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_postal_status; text="postal status"; type="editbox"; form=p_location_form; tooltip="e.g., ACS status, CASS status -- K=Addressee-UnKnown F=Forwarding-Order-Expired N=No-such-#-or-Address U=Undeliverable-by-P.O. X=Other"; }
                    p_postal_barcode_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_postal_barcode; text="postal barcode"; type="editbox"; form=p_location_form; tooltip="Postal barcode sequence for printing mailing labels etc. -- "; }
                    p_record_status_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_record_status_code; text="record status code"; type="editbox"; form=p_location_form; tooltip="A=active, O=obsolete, Q=active but needs QA (e.g., new record) -- "; }
                    }
                p_location_hbox7 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_location_comments_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_location_comments; text="location comments"; type="editbox"; form=p_location_form; tooltip=" -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=p_location_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=p_location_form; tooltip=" -- "; }
                    }
                p_location_hbox8 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=p_location_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=p_location_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=p_location_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
