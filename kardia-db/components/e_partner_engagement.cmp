$Version=2$
e_partner_engagement_component "widget/component-decl"
    {
    width=835;height=635;
    e_partner_engagement_osrc "widget/osrc"
        {
        sql="SELECT :e_completion_date, :e_step_id, :e_exited_by, :e_completed_by, :e_started_by, :e_start_date, :p_partner_key, :e_desc, :__cx_osml_control, :e_engagement_id, :s_modified_by, :e_track_id, :s_date_created, :e_is_archived, :s_date_modified, :e_exited_date, :s_created_by, :e_hist_id, :e_completion_status, :e_comments FROM /lightsys/partner_DB/e_partner_engagement/rows";
        baseobj = "/lightsys/partner_DB/e_partner_engagement/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        e_partner_engagement_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            e_partner_engagement_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            e_partner_engagement_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                e_partner_engagement_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_partner_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_partner_key; text="partner key"; type="editbox"; form=e_partner_engagement_form; tooltip="The partner ID that is doing the engaging -- "; }
                    e_engagement_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_engagement_id; text="engagement id"; type="editbox"; form=e_partner_engagement_form; tooltip="A unique ID for this partner's current engagement -- "; }
                    e_hist_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_hist_id; text="hist id"; type="editbox"; form=e_partner_engagement_form; tooltip="An incrementing ID for showing the history of this engagement. -- "; }
                    }
                e_partner_engagement_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_track_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_track_id; text="track id"; type="editbox"; form=e_partner_engagement_form; tooltip="The engagement track -- "; }
                    e_step_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_step_id; text="step id"; type="editbox"; form=e_partner_engagement_form; tooltip="The current engagement step -- "; }
                    e_is_archived_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_is_archived; text="is archived"; type="editbox"; form=e_partner_engagement_form; tooltip="Set to 0 if the engagement in this track is currently active, or 1 if it is archived/historical -- "; }
                    }
                e_partner_engagement_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_completion_status_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_completion_status; text="completion status"; type="editbox"; form=e_partner_engagement_form; tooltip="Status of the step: (I)ncomplete, (C)omplete, (E)xited without completing the step -- "; }
                    e_desc_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_desc; text="desc"; type="editbox"; form=e_partner_engagement_form; tooltip="User-entered short description on this engagement track/step for this partner -- "; }
                    e_comments_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_comments; text="comments"; type="editbox"; form=e_partner_engagement_form; tooltip="User-entered comments on this engagement track/step for this partner -- "; }
                    }
                e_partner_engagement_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_start_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_start_date; text="start date"; type="editbox"; form=e_partner_engagement_form; tooltip="The date the user entered this engagement track and/or step -- "; }
                    e_started_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_started_by; text="started by"; type="editbox"; form=e_partner_engagement_form; tooltip="Collaborator partner ID that caused the track/step to begin -- "; }
                    e_completion_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_completion_date; text="completion date"; type="editbox"; form=e_partner_engagement_form; tooltip="The date the user completed this engagement step -- "; }
                    }
                e_partner_engagement_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_completed_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_completed_by; text="completed by"; type="editbox"; form=e_partner_engagement_form; tooltip="Collaborator partner ID that caused the track/step to become complete -- "; }
                    e_exited_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_exited_date; text="exited date"; type="editbox"; form=e_partner_engagement_form; tooltip="The date the user exited this engagement step (whether completed or not) -- "; }
                    e_exited_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_exited_by; text="exited by"; type="editbox"; form=e_partner_engagement_form; tooltip="Collaborator partner ID that caused the track/step to exit -- "; }
                    }
                e_partner_engagement_hbox5 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=e_partner_engagement_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=e_partner_engagement_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=e_partner_engagement_form; tooltip=" -- "; }
                    }
                e_partner_engagement_hbox6 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=e_partner_engagement_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=e_partner_engagement_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
