$Version=2$
e_collaborator_component "widget/component-decl"
    {
    width=835;height=635;
    e_collaborator_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :e_collaborator_status, :e_collab_type_id, :s_date_created, :e_collaborator, :s_date_modified, :p_partner_key, :s_created_by, :e_recontact_interval, :__cx_osml_control, :e_is_automatic, :e_silence_interval FROM /lightsys/partner_DB/e_collaborator/rows";
        baseobj = "/lightsys/partner_DB/e_collaborator/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        e_collaborator_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            e_collaborator_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            e_collaborator_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                e_collaborator_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_collaborator_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_collaborator; text="collaborator"; type="editbox"; form=e_collaborator_form; tooltip="The ID (partner key) of the collaborator -- "; }
                    p_partner_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_partner_key; text="partner key"; type="editbox"; form=e_collaborator_form; tooltip="The id (partner key) of the partner who is engaging -- "; }
                    e_collab_type_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_collab_type_id; text="collab type id"; type="editbox"; form=e_collaborator_form; tooltip="The collaboration type ID -- "; }
                    }
                e_collaborator_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_silence_interval_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_silence_interval; text="silence interval"; type="editbox"; form=e_collaborator_form; tooltip="The number of days without communication FROM a partner before a task item is generated for re-contacting the partner. Communication here can include all non-note contact history types, as well as financial gifts received. -- "; }
                    e_recontact_interval_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_recontact_interval; text="recontact interval"; type="editbox"; form=e_collaborator_form; tooltip="Assuming the e_silence_interval, above, is met, this is the number of days since OUR last communication TO the partner before another task item is generated to re-contact the partner -- "; }
                    e_collaborator_status_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_collaborator_status; text="collaborator status"; type="editbox"; form=e_collaborator_form; tooltip="Collaborator status: (P) Priority, (A) Active, (I) Inactive ("back burner"). -- "; }
                    }
                e_collaborator_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_is_automatic_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_is_automatic; text="is automatic"; type="editbox"; form=e_collaborator_form; tooltip="Set to 1 if this was added automatically by a track or step. -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=e_collaborator_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=e_collaborator_form; tooltip=" -- "; }
                    }
                e_collaborator_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=e_collaborator_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=e_collaborator_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=e_collaborator_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
