$Version=2$
t_sprint_component "widget/component-decl"
    {
    width=835;height=635;
    t_sprint_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :t_sprint_label, :t_project_id, :t_sprint_start, :s_date_created, :t_sprint_end, :s_date_modified, :s_created_by, :__cx_osml_control, :t_sprint_id FROM /lightsys/partner_DB/t_sprint/rows";
        baseobj = "/lightsys/partner_DB/t_sprint/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        t_sprint_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            t_sprint_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            t_sprint_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                t_sprint_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    t_sprint_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=t_sprint_id; text="sprint id"; type="editbox"; form=t_sprint_form; tooltip="unique sprint ID -- "; }
                    t_project_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=t_project_id; text="project id"; type="editbox"; form=t_sprint_form; tooltip="unique project ID -- "; }
                    t_sprint_label_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=t_sprint_label; text="sprint label"; type="editbox"; form=t_sprint_form; tooltip="a short label (name) for the sprint. -- "; }
                    }
                t_sprint_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    t_sprint_start_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=t_sprint_start; text="sprint start"; type="editbox"; form=t_sprint_form; tooltip="starting date for the sprint -- "; }
                    t_sprint_end_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=t_sprint_end; text="sprint end"; type="editbox"; form=t_sprint_form; tooltip="ending date for the sprint -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=t_sprint_form; tooltip=" -- "; }
                    }
                t_sprint_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=t_sprint_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=t_sprint_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=t_sprint_form; tooltip=" -- "; }
                    }
                t_sprint_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=t_sprint_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
