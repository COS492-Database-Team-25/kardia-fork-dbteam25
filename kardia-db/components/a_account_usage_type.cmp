$Version=2$
a_account_usage_type_component "widget/component-decl"
    {
    width=835;height=635;
    a_account_usage_type_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :a_acct_usage_code, :s_date_created, :a_acct_type, :s_date_modified, :s_created_by, :__cx_osml_control, :a_acct_usage_desc, :a_acct_usage_comment FROM /lightsys/partner_DB/a_account_usage_type/rows";
        baseobj = "/lightsys/partner_DB/a_account_usage_type/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        a_account_usage_type_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            a_account_usage_type_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            a_account_usage_type_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                a_account_usage_type_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_acct_usage_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_acct_usage_code; text="acct usage code"; type="editbox"; form=a_account_usage_type_form; tooltip="account usage code (alphanumeric allowed) -- "; }
                    a_acct_type_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_acct_type; text="acct type"; type="editbox"; form=a_account_usage_type_form; tooltip="type of account that must be selected: (A)sset, (L)iability, (Q)Equity, (R)evenue, (E)xpense -- "; }
                    a_acct_usage_desc_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_acct_usage_desc; text="acct usage desc"; type="editbox"; form=a_account_usage_type_form; tooltip="short description of account usage for reporting -- "; }
                    }
                a_account_usage_type_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_acct_usage_comment_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_acct_usage_comment; text="acct usage comment"; type="editbox"; form=a_account_usage_type_form; tooltip="comments / long description of account usage -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=a_account_usage_type_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=a_account_usage_type_form; tooltip=" -- "; }
                    }
                a_account_usage_type_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=a_account_usage_type_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=a_account_usage_type_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=a_account_usage_type_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
