$Version=2$
p_partner_search_component "widget/component-decl"
    {
    width=835;height=635;
    p_partner_search_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :p_search_visibility, :p_is_temporary, :s_date_created, :p_search_id, :s_date_modified, :p_search_desc, :s_created_by, :__cx_osml_control FROM /lightsys/partner_DB/p_partner_search/rows";
        baseobj = "/lightsys/partner_DB/p_partner_search/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        p_partner_search_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            p_partner_search_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            p_partner_search_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                p_partner_search_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_search_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_search_id; text="search id"; type="editbox"; form=p_partner_search_form; tooltip="ID of search, a unique integer. -- "; }
                    p_search_desc_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_search_desc; text="search desc"; type="editbox"; form=p_partner_search_form; tooltip="A name/description given to this search -- "; }
                    p_search_visibility_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_search_visibility; text="search visibility"; type="editbox"; form=p_partner_search_form; tooltip="P = only to me, A = visible to everyone -- "; }
                    }
                p_partner_search_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_is_temporary_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_is_temporary; text="is temporary"; type="editbox"; form=p_partner_search_form; tooltip="If set to 1 (true), then this search and its results will automatically be erased -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=p_partner_search_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=p_partner_search_form; tooltip=" -- "; }
                    }
                p_partner_search_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=p_partner_search_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=p_partner_search_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=p_partner_search_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
