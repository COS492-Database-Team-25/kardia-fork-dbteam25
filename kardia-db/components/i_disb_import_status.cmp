$Version=2$
i_disb_import_status_component "widget/component-decl"
    {
    width=835;height=635;
    i_disb_import_status_osrc "widget/osrc"
        {
        sql="SELECT :i_disb_legacy_key, :i_disb_import_status, :i_disb_legacy_approveddate, :i_disb_legacy_payee, :__cx_osml_control, :i_disb_legacy_entereddate, :i_disb_legacy_paydate, :a_ledger_number, :i_disb_partner_key, :s_modified_by, :i_disb_legacy_enteredby, :i_disb_legacy_ckno, :i_disb_legacy_amount, :i_disb_import_comments, :i_disb_legacy_approvedby, :s_date_created, :s_date_modified, :s_created_by FROM /lightsys/partner_DB/i_disb_import_status/rows";
        baseobj = "/lightsys/partner_DB/i_disb_import_status/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        i_disb_import_status_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            i_disb_import_status_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            i_disb_import_status_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                i_disb_import_status_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_ledger_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_ledger_number; text="ledger number"; type="editbox"; form=i_disb_import_status_form; tooltip="ledger number for this classification rule -- "; }
                    i_disb_legacy_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_legacy_key; text="disb legacy key"; type="editbox"; form=i_disb_import_status_form; tooltip="unique key for legacy data -- "; }
                    i_disb_legacy_amount_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_legacy_amount; text="disb legacy amount"; type="editbox"; form=i_disb_import_status_form; tooltip="The amount of the legacy data transaction -- "; }
                    }
                i_disb_import_status_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    i_disb_legacy_payee_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_legacy_payee; text="disb legacy payee"; type="editbox"; form=i_disb_import_status_form; tooltip="Payee ID in legacy system -- "; }
                    i_disb_partner_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_partner_key; text="disb partner key"; type="editbox"; form=i_disb_import_status_form; tooltip="Partner key from Kardia. Null if there is no match based on Legacy Key 1. -- "; }
                    i_disb_legacy_ckno_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_legacy_ckno; text="disb legacy ckno"; type="editbox"; form=i_disb_import_status_form; tooltip="Check number in legacy system -- "; }
                    }
                i_disb_import_status_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    i_disb_legacy_paydate_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_legacy_paydate; text="disb legacy paydate"; type="editbox"; form=i_disb_import_status_form; tooltip="Payment date in legacy system -- "; }
                    i_disb_legacy_entereddate_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_legacy_entereddate; text="disb legacy entereddate"; type="editbox"; form=i_disb_import_status_form; tooltip="Entered date from legacy system -- "; }
                    i_disb_legacy_approveddate_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_legacy_approveddate; text="disb legacy approveddate"; type="editbox"; form=i_disb_import_status_form; tooltip="Approval date from legacy system -- "; }
                    }
                i_disb_import_status_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    i_disb_legacy_enteredby_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_legacy_enteredby; text="disb legacy enteredby"; type="editbox"; form=i_disb_import_status_form; tooltip="Who entered the data in the legacy system. -- "; }
                    i_disb_legacy_approvedby_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_legacy_approvedby; text="disb legacy approvedby"; type="editbox"; form=i_disb_import_status_form; tooltip="Who approved the data in the legacy system. -- "; }
                    i_disb_import_status_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_import_status; text="disb import status"; type="editbox"; form=i_disb_import_status_form; tooltip="Import status: (N) not imported, (I) imported, (S) selected for current import -- "; }
                    }
                i_disb_import_status_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    i_disb_import_comments_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_import_comments; text="disb import comments"; type="editbox"; form=i_disb_import_status_form; tooltip="Comments -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=i_disb_import_status_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=i_disb_import_status_form; tooltip=" -- "; }
                    }
                i_disb_import_status_hbox5 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=i_disb_import_status_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=i_disb_import_status_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=i_disb_import_status_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
