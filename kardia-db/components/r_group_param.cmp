$Version=2$
r_group_param_component "widget/component-decl"
    {
    width=835;height=635;
    r_group_param_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :r_param_default, :s_date_created, :r_param_cmp_module, :s_date_modified, :s_created_by, :__cx_osml_control, :r_group_name, :r_param_name, :r_param_description, :r_is_required, :r_is_group_param, :r_is_report_param, :r_param_cmp_file FROM /lightsys/partner_DB/r_group_param/rows";
        baseobj = "/lightsys/partner_DB/r_group_param/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        r_group_param_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            r_group_param_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            r_group_param_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                r_group_param_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    r_group_name_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=r_group_name; text="group name"; type="editbox"; form=r_group_param_form; tooltip="short name of the report group that this parameter belongs to -- "; }
                    r_param_name_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=r_param_name; text="param name"; type="editbox"; form=r_group_param_form; tooltip="name of the parameter -- "; }
                    r_param_description_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=r_param_description; text="param description"; type="editbox"; form=r_group_param_form; tooltip="description of the report parameter -- "; }
                    }
                r_group_param_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    r_is_group_param_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=r_is_group_param; text="is group param"; type="editbox"; form=r_group_param_form; tooltip="whether this parameter can be supplied by the user running the report group -- "; }
                    r_is_report_param_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=r_is_report_param; text="is report param"; type="editbox"; form=r_group_param_form; tooltip="whether this parameter can be supplied by individual reports in the group -- "; }
                    r_is_required_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=r_is_required; text="is required"; type="editbox"; form=r_group_param_form; tooltip="whether this parameter MUST be supplied -- "; }
                    }
                r_group_param_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    r_param_cmp_module_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=r_param_cmp_module; text="param cmp module"; type="editbox"; form=r_group_param_form; tooltip="component module (directory name) used for getting user input on this parameter. -- "; }
                    r_param_cmp_file_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=r_param_cmp_file; text="param cmp file"; type="editbox"; form=r_group_param_form; tooltip="component file (.cmp) used for getting user input on this parameter. -- "; }
                    r_param_default_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=r_param_default; text="param default"; type="editbox"; form=r_group_param_form; tooltip="default value for the parameter (in string format). -- "; }
                    }
                r_group_param_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=r_group_param_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=r_group_param_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=r_group_param_form; tooltip=" -- "; }
                    }
                r_group_param_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=r_group_param_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=r_group_param_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
