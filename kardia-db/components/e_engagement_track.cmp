$Version=2$
e_engagement_track_component "widget/component-decl"
    {
    width=835;height=635;
    e_engagement_track_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :e_track_color, :e_track_name, :e_track_id, :s_date_created, :e_track_description, :e_track_status, :s_date_modified, :s_created_by, :__cx_osml_control FROM /lightsys/partner_DB/e_engagement_track/rows";
        baseobj = "/lightsys/partner_DB/e_engagement_track/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        e_engagement_track_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            e_engagement_track_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            e_engagement_track_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                e_engagement_track_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_track_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_track_id; text="track id"; type="editbox"; form=e_engagement_track_form; tooltip="A unique ID identifying the engagement track -- "; }
                    e_track_name_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_track_name; text="track name"; type="editbox"; form=e_engagement_track_form; tooltip="A short name for the engagement track -- "; }
                    e_track_description_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_track_description; text="track description"; type="editbox"; form=e_engagement_track_form; tooltip="A description for the engagement track -- "; }
                    }
                e_engagement_track_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_track_color_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_track_color; text="track color"; type="editbox"; form=e_engagement_track_form; tooltip="Color to be used in presenting this track visually - RGB (i.e. '#f0f0f0') or named (i.e., 'green') -- "; }
                    e_track_status_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_track_status; text="track status"; type="editbox"; form=e_engagement_track_form; tooltip="Status of this engagement track: (A)ctive or (O)bsolete -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=e_engagement_track_form; tooltip=" -- "; }
                    }
                e_engagement_track_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=e_engagement_track_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=e_engagement_track_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=e_engagement_track_form; tooltip=" -- "; }
                    }
                e_engagement_track_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=e_engagement_track_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
