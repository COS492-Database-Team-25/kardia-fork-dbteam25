$Version=2$
a_tax_table_component "widget/component-decl"
    {
    width=835;height=635;
    a_tax_table_osrc "widget/osrc"
        {
        sql="SELECT :a_minimum_salary, :a_add_to_tax, :__cx_osml_control, :a_payroll_item_type, :a_ledger_number, :s_modified_by, :a_tax_entry_id, :s_date_created, :a_filing_status, :a_percent, :s_date_modified, :a_maximum_salary, :s_created_by, :a_payroll_interval, :a_end_date, :a_start_date, :a_subtract_salary FROM /lightsys/partner_DB/a_tax_table/rows";
        baseobj = "/lightsys/partner_DB/a_tax_table/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        a_tax_table_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            a_tax_table_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            a_tax_table_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                a_tax_table_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_tax_entry_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_tax_entry_id; text="tax entry id"; type="editbox"; form=a_tax_table_form; tooltip="synthetic primary key -- "; }
                    a_payroll_item_type_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_payroll_item_type; text="payroll item type"; type="editbox"; form=a_tax_table_form; tooltip="payroll item type code that uses this tax table (of type 'T') -- "; }
                    a_ledger_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_ledger_number; text="ledger number"; type="editbox"; form=a_tax_table_form; tooltip="ledger number (alphanumeric allowed) -- "; }
                    }
                a_tax_table_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_start_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_start_date; text="start date"; type="editbox"; form=a_tax_table_form; tooltip="beginning date that this table is valid. -- "; }
                    a_end_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_end_date; text="end date"; type="editbox"; form=a_tax_table_form; tooltip="last date that this table is valid. -- "; }
                    a_payroll_interval_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_payroll_interval; text="payroll interval"; type="editbox"; form=a_tax_table_form; tooltip="interval (OW = once weekly, BW = biweekly, OD = daily, SM = semimonthly, OM = once monthly, MS = misc) -- "; }
                    }
                a_tax_table_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_filing_status_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_filing_status; text="filing status"; type="editbox"; form=a_tax_table_form; tooltip="for tax withholding, the filing status. -- "; }
                    a_minimum_salary_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_minimum_salary; text="minimum salary"; type="editbox"; form=a_tax_table_form; tooltip="minimum salary amount that this item applies to -- "; }
                    a_maximum_salary_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_maximum_salary; text="maximum salary"; type="editbox"; form=a_tax_table_form; tooltip="maximum salary amount that this item applies to -- "; }
                    }
                a_tax_table_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_subtract_salary_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_subtract_salary; text="subtract salary"; type="editbox"; form=a_tax_table_form; tooltip="amount to subtract from salary before computing percentage -- "; }
                    a_percent_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_percent; text="percent"; type="editbox"; form=a_tax_table_form; tooltip="percent of tax -- "; }
                    a_add_to_tax_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_add_to_tax; text="add to tax"; type="editbox"; form=a_tax_table_form; tooltip="amount to add to tax after percent is computed -- "; }
                    }
                a_tax_table_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=a_tax_table_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=a_tax_table_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=a_tax_table_form; tooltip=" -- "; }
                    }
                a_tax_table_hbox5 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=a_tax_table_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=a_tax_table_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
