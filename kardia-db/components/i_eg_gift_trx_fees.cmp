$Version=2$
i_eg_gift_trx_fees_component "widget/component-decl"
    {
    width=835;height=635;
    i_eg_gift_trx_fees_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :a_ledger_number, :i_eg_fees_id, :i_eg_gift_currency, :s_date_created, :s_date_modified, :i_eg_gift_pmt_type, :s_created_by, :__cx_osml_control, :i_eg_processor, :i_eg_fee_pct_amt, :i_eg_fee_flat_amt, :i_eg_service FROM /lightsys/partner_DB/i_eg_gift_trx_fees/rows";
        baseobj = "/lightsys/partner_DB/i_eg_gift_trx_fees/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        i_eg_gift_trx_fees_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            i_eg_gift_trx_fees_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            i_eg_gift_trx_fees_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                i_eg_gift_trx_fees_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_ledger_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_ledger_number; text="ledger number"; type="editbox"; form=i_eg_gift_trx_fees_form; tooltip="ledger number for this set of fees -- "; }
                    i_eg_fees_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_eg_fees_id; text="eg fees id"; type="editbox"; form=i_eg_gift_trx_fees_form; tooltip="A unique ID for the fees data. -- "; }
                    i_eg_service_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_eg_service; text="eg service"; type="editbox"; form=i_eg_gift_trx_fees_form; tooltip="Service ID (e.g. EG, EGS, SS) from Kardia online giving service plugin -- "; }
                    }
                i_eg_gift_trx_fees_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    i_eg_processor_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_eg_processor; text="eg processor"; type="editbox"; form=i_eg_gift_trx_fees_form; tooltip="Name of payment processor (xml:processor) -- "; }
                    i_eg_gift_currency_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_eg_gift_currency; text="eg gift currency"; type="editbox"; form=i_eg_gift_trx_fees_form; tooltip="currency of gift (e.g. USD, CAD, etc) -- "; }
                    i_eg_gift_pmt_type_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_eg_gift_pmt_type; text="eg gift pmt type"; type="editbox"; form=i_eg_gift_trx_fees_form; tooltip="Payment type (xml:payment-type) -- "; }
                    }
                i_eg_gift_trx_fees_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    i_eg_fee_flat_amt_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_eg_fee_flat_amt; text="eg fee flat amt"; type="editbox"; form=i_eg_gift_trx_fees_form; tooltip="Flat part of fee -- "; }
                    i_eg_fee_pct_amt_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_eg_fee_pct_amt; text="eg fee pct amt"; type="editbox"; form=i_eg_gift_trx_fees_form; tooltip="Percentage part of fee -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=i_eg_gift_trx_fees_form; tooltip=" -- "; }
                    }
                i_eg_gift_trx_fees_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=i_eg_gift_trx_fees_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=i_eg_gift_trx_fees_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=i_eg_gift_trx_fees_form; tooltip=" -- "; }
                    }
                i_eg_gift_trx_fees_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=i_eg_gift_trx_fees_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
