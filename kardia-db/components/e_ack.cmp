$Version=2$
e_ack_component "widget/component-decl"
    {
    width=835;height=635;
    e_ack_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :e_object_id, :p_dn_partner_key, :e_ack_type, :e_ack_comments, :s_date_created, :s_date_modified, :e_ack_id, :s_created_by, :__cx_osml_control, :e_whom, :e_object_type FROM /lightsys/partner_DB/e_ack/rows";
        baseobj = "/lightsys/partner_DB/e_ack/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        e_ack_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            e_ack_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            e_ack_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                e_ack_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_ack_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_ack_id; text="ack id"; type="editbox"; form=e_ack_form; tooltip="The unique ID of this acknowledgement -- "; }
                    e_object_type_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_object_type; text="object type"; type="editbox"; form=e_ack_form; tooltip="The type of object that has been acknowledged -- "; }
                    e_object_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_object_id; text="object id"; type="editbox"; form=e_ack_form; tooltip="A unique ID identifying the object acknowledged -- "; }
                    }
                e_ack_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_ack_type_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_ack_type; text="ack type"; type="editbox"; form=e_ack_form; tooltip="The type of acknowledgement (viewed, prayed, comment, like, etc.) -- "; }
                    e_ack_comments_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_ack_comments; text="ack comments"; type="editbox"; form=e_ack_form; tooltip="Comments related to this ACK (for example, if the ack is a comment on a post) -- "; }
                    e_whom_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_whom; text="whom"; type="editbox"; form=e_ack_form; tooltip="The collaborator who acknowledged the object -- "; }
                    }
                e_ack_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_dn_partner_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_dn_partner_key; text="dn partner key"; type="editbox"; form=e_ack_form; tooltip="(denormalization) the partner key of the person who created the object being acknowledged. -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=e_ack_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=e_ack_form; tooltip=" -- "; }
                    }
                e_ack_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=e_ack_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=e_ack_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=e_ack_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
