$Version=2$
a_payroll_item_type_component "widget/component-decl"
    {
    width=835;height=635;
    a_payroll_item_type_osrc "widget/osrc"
        {
        sql="SELECT :a_exempt_from_tax, :__cx_osml_control, :a_comment, :a_xfer_account_code, :a_xfer_cost_center, :a_state_province, :a_ledger_number, :s_modified_by, :a_ref_account_code, :s_date_created, :a_payroll_item_class_code, :a_desc, :s_date_modified, :a_payroll_item_type_code, :s_created_by, :a_payroll_item_form_sequence FROM /lightsys/partner_DB/a_payroll_item_type/rows";
        baseobj = "/lightsys/partner_DB/a_payroll_item_type/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        a_payroll_item_type_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            a_payroll_item_type_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            a_payroll_item_type_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                a_payroll_item_type_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_ledger_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_ledger_number; text="ledger number"; type="editbox"; form=a_payroll_item_type_form; tooltip="ledger number that will be doing the payroll. -- "; }
                    a_payroll_item_type_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_payroll_item_type_code; text="payroll item type code"; type="editbox"; form=a_payroll_item_type_form; tooltip="code for this payroll item type -- "; }
                    a_payroll_item_class_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_payroll_item_class_code; text="payroll item class code"; type="editbox"; form=a_payroll_item_type_form; tooltip="general category of payroll item -- "; }
                    }
                a_payroll_item_type_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_payroll_item_form_sequence_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_payroll_item_form_sequence; text="payroll item form sequence"; type="editbox"; form=a_payroll_item_type_form; tooltip="order this item comes in on the payroll form. -- "; }
                    a_ref_account_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_ref_account_code; text="ref account code"; type="editbox"; form=a_payroll_item_type_form; tooltip="default GL account code to use for items of this type -- "; }
                    a_xfer_cost_center_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_xfer_cost_center; text="xfer cost center"; type="editbox"; form=a_payroll_item_type_form; tooltip="default cost center to use when this item involves an xfer -- "; }
                    }
                a_payroll_item_type_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_xfer_account_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_xfer_account_code; text="xfer account code"; type="editbox"; form=a_payroll_item_type_form; tooltip="default gl account to use when this item involves an xfer -- "; }
                    a_state_province_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_state_province; text="state province"; type="editbox"; form=a_payroll_item_type_form; tooltip="State/Province that this tax table relates to. -- "; }
                    a_desc_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_desc; text="desc"; type="editbox"; form=a_payroll_item_type_form; tooltip="description for this code -- "; }
                    }
                a_payroll_item_type_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_exempt_from_tax_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_exempt_from_tax; text="exempt from tax"; type="editbox"; form=a_payroll_item_type_form; tooltip="if this line item is excluded from tax (for "G"ross or for post-tax deductions), or included in tax (for "pre-tax" deductions), this is a list of tax line item types that are excluded/included. -- "; }
                    a_comment_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_comment; text="comment"; type="editbox"; form=a_payroll_item_type_form; tooltip="comments for this code -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=a_payroll_item_type_form; tooltip=" -- "; }
                    }
                a_payroll_item_type_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=a_payroll_item_type_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=a_payroll_item_type_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=a_payroll_item_type_form; tooltip=" -- "; }
                    }
                a_payroll_item_type_hbox5 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=a_payroll_item_type_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
