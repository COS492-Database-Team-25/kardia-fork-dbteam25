$Version=2$
a_giving_pattern_component "widget/component-decl"
    {
    width=835;height=635;
    a_giving_pattern_osrc "widget/osrc"
        {
        sql="SELECT :a_amount, :a_history_id, :a_pattern_id, :p_donor_partner_key, :a_cost_center, :__cx_osml_control, :a_comment, :a_evaluation_date, :a_review, :a_interval, :s_modified_by, :a_ledger_number, :a_is_active, :a_actual_cost_center, :s_date_created, :a_percent, :s_date_modified, :s_created_by, :a_start_date, :a_end_date FROM /lightsys/partner_DB/a_giving_pattern/rows";
        baseobj = "/lightsys/partner_DB/a_giving_pattern/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        a_giving_pattern_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            a_giving_pattern_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            a_giving_pattern_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                a_giving_pattern_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_ledger_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_ledger_number; text="ledger number"; type="editbox"; form=a_giving_pattern_form; tooltip="ledger number for this giving pattern -- "; }
                    p_donor_partner_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_donor_partner_key; text="donor partner key"; type="editbox"; form=a_giving_pattern_form; tooltip="Partner ID for the donor -- "; }
                    a_cost_center_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_cost_center; text="cost center"; type="editbox"; form=a_giving_pattern_form; tooltip="fund that this donor is giving toward -- "; }
                    }
                a_giving_pattern_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_pattern_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_pattern_id; text="pattern id"; type="editbox"; form=a_giving_pattern_form; tooltip="sequential integer ID of this giving pattern record. -- "; }
                    a_history_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_history_id; text="history id"; type="editbox"; form=a_giving_pattern_form; tooltip="sequential integer ID of the history of this giving pattern record. -- "; }
                    a_review_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_review; text="review"; type="editbox"; form=a_giving_pattern_form; tooltip="An identifier for the support review this entry is associated with -- "; }
                    }
                a_giving_pattern_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_amount_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_amount; text="amount"; type="editbox"; form=a_giving_pattern_form; tooltip="Amount of money the donor is giving -- "; }
                    a_interval_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_interval; text="interval"; type="editbox"; form=a_giving_pattern_form; tooltip="Month interval at which the donor is giving (1=monthly, 3=quarterly, 12=annually, 0=onetime/as-able, null=unknown) -- "; }
                    a_is_active_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_is_active; text="is active"; type="editbox"; form=a_giving_pattern_form; tooltip="Is this giving pattern an active one? -- "; }
                    }
                a_giving_pattern_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_start_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_start_date; text="start date"; type="editbox"; form=a_giving_pattern_form; tooltip="Date that this donor began this current giving pattern -- "; }
                    a_end_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_end_date; text="end date"; type="editbox"; form=a_giving_pattern_form; tooltip="Date that we expect the donor to end this giving pattern (NOT A LEGAL OBLIGATION ON THE DONOR'S PART) -- "; }
                    a_evaluation_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_evaluation_date; text="evaluation date"; type="editbox"; form=a_giving_pattern_form; tooltip="Date that this giving pattern was determined -- "; }
                    }
                a_giving_pattern_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_actual_cost_center_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_actual_cost_center; text="actual cost center"; type="editbox"; form=a_giving_pattern_form; tooltip="Actual cost center that the donor donated to, if not to the one listed above. -- "; }
                    a_percent_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_percent; text="percent"; type="editbox"; form=a_giving_pattern_form; tooltip="Percent of gift (0.00 to 1.00) allocated to this fund, if a_actual_cost_center is supplied. -- "; }
                    a_comment_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_comment; text="comment"; type="editbox"; form=a_giving_pattern_form; tooltip="Giving pattern comments -- "; }
                    }
                a_giving_pattern_hbox5 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=a_giving_pattern_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=a_giving_pattern_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=a_giving_pattern_form; tooltip=" -- "; }
                    }
                a_giving_pattern_hbox6 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=a_giving_pattern_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=a_giving_pattern_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
