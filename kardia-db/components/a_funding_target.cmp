$Version=2$
a_funding_target_component "widget/component-decl"
    {
    width=835;height=635;
    a_funding_target_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :a_amount, :a_ledger_number, :s_date_created, :s_date_modified, :a_target_id, :a_target_desc, :s_created_by, :a_cost_center, :__cx_osml_control, :a_end_date, :a_review, :a_interval, :a_start_date FROM /lightsys/partner_DB/a_funding_target/rows";
        baseobj = "/lightsys/partner_DB/a_funding_target/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        a_funding_target_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            a_funding_target_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            a_funding_target_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                a_funding_target_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_ledger_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_ledger_number; text="ledger number"; type="editbox"; form=a_funding_target_form; tooltip="ledger number for the fund -- "; }
                    a_cost_center_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_cost_center; text="cost center"; type="editbox"; form=a_funding_target_form; tooltip="fund that we're establishing a target for -- "; }
                    a_target_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_target_id; text="target id"; type="editbox"; form=a_funding_target_form; tooltip="sequential integer ID of this funding target record. -- "; }
                    }
                a_funding_target_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_target_desc_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_target_desc; text="target desc"; type="editbox"; form=a_funding_target_form; tooltip="Description for this funding target -- "; }
                    a_review_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_review; text="review"; type="editbox"; form=a_funding_target_form; tooltip="Support review ID that this target is connected with, if available -- "; }
                    a_amount_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_amount; text="amount"; type="editbox"; form=a_funding_target_form; tooltip="Amount of money for the target -- "; }
                    }
                a_funding_target_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_interval_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_interval; text="interval"; type="editbox"; form=a_funding_target_form; tooltip="Month interval for the funding target (0=absolute, 1=monthly, 3=quarterly, 12=annually) -- "; }
                    a_start_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_start_date; text="start date"; type="editbox"; form=a_funding_target_form; tooltip="Date that this funding target begins -- "; }
                    a_end_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_end_date; text="end date"; type="editbox"; form=a_funding_target_form; tooltip="Date that this funding target ends -- "; }
                    }
                a_funding_target_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=a_funding_target_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=a_funding_target_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=a_funding_target_form; tooltip=" -- "; }
                    }
                a_funding_target_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=a_funding_target_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=a_funding_target_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
