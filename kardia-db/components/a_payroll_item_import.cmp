$Version=2$
a_payroll_item_import_component "widget/component-decl"
    {
    width=835;height=635;
    a_payroll_item_import_osrc "widget/osrc"
        {
        sql="SELECT :a_minimum_amount, :a_ref_cost_center, :a_dependent_allowances, :a_effective_date, :a_period, :a_payroll_item_id, :__cx_osml_control, :a_xfer_account_code, :a_xfer_cost_center, :a_is_instance, :s_modified_by, :a_payroll_id, :a_ref_account_code, :s_date_created, :a_filing_status, :a_percent, :s_date_modified, :s_created_by, :a_target_amount, :a_payroll_item_type_code, :a_allowances, :a_actual_amount, :a_end_date, :a_start_date FROM /lightsys/partner_DB/a_payroll_item_import/rows";
        baseobj = "/lightsys/partner_DB/a_payroll_item_import/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        a_payroll_item_import_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            a_payroll_item_import_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            a_payroll_item_import_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                a_payroll_item_import_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_payroll_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_payroll_id; text="payroll id"; type="editbox"; form=a_payroll_item_import_form; tooltip="unique ID for this payroll entry -> a_payroll:a_payroll_id -- "; }
                    a_payroll_item_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_payroll_item_id; text="payroll item id"; type="editbox"; form=a_payroll_item_import_form; tooltip="unique ID for this payroll item -- "; }
                    a_payroll_item_type_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_payroll_item_type_code; text="payroll item type code"; type="editbox"; form=a_payroll_item_import_form; tooltip="type of payroll item -> a_payroll_item_type -- "; }
                    }
                a_payroll_item_import_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_is_instance_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_is_instance; text="is instance"; type="editbox"; form=a_payroll_item_import_form; tooltip="set to 1 if this is an actual instance of payroll, rather than the template -- "; }
                    a_period_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_period; text="period"; type="editbox"; form=a_payroll_item_import_form; tooltip="if this is an instance, a_period contains the relevant accounting period -- "; }
                    a_effective_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_effective_date; text="effective date"; type="editbox"; form=a_payroll_item_import_form; tooltip="if this is an instance, this field specifies the effective date for the payroll -- "; }
                    }
                a_payroll_item_import_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_target_amount_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_target_amount; text="target amount"; type="editbox"; form=a_payroll_item_import_form; tooltip="target amount of currency, such as amount for gross pay -- "; }
                    a_actual_amount_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_actual_amount; text="actual amount"; type="editbox"; form=a_payroll_item_import_form; tooltip="actual amount of currency used, for payroll item instances -- "; }
                    a_minimum_amount_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_minimum_amount; text="minimum amount"; type="editbox"; form=a_payroll_item_import_form; tooltip="if amount is flexible, this specifies the minimum -- "; }
                    }
                a_payroll_item_import_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_percent_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_percent; text="percent"; type="editbox"; form=a_payroll_item_import_form; tooltip="if this is a percentage-based item, the percent goes here -- "; }
                    a_filing_status_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_filing_status; text="filing status"; type="editbox"; form=a_payroll_item_import_form; tooltip="for tax withholding, the filing status. -- "; }
                    a_allowances_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_allowances; text="allowances"; type="editbox"; form=a_payroll_item_import_form; tooltip="for tax withholding, the number of withholding allowances claimed (e.g., on W4) -- "; }
                    }
                a_payroll_item_import_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_dependent_allowances_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_dependent_allowances; text="dependent allowances"; type="editbox"; form=a_payroll_item_import_form; tooltip="for tax withholding, the number of withholding allowances claimed for dependents (some states, such as Georgia/Indiana) -- "; }
                    a_ref_cost_center_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_ref_cost_center; text="ref cost center"; type="editbox"; form=a_payroll_item_import_form; tooltip="for receivables/payables/etc, the cost center to check -- "; }
                    a_ref_account_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_ref_account_code; text="ref account code"; type="editbox"; form=a_payroll_item_import_form; tooltip="for receivables/payables/etc, the gl account to check -- "; }
                    }
                a_payroll_item_import_hbox5 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_xfer_cost_center_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_xfer_cost_center; text="xfer cost center"; type="editbox"; form=a_payroll_item_import_form; tooltip="for line items that reference an outside cost ctr / fund. -- "; }
                    a_xfer_account_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_xfer_account_code; text="xfer account code"; type="editbox"; form=a_payroll_item_import_form; tooltip="gl acct to use in outside cost ctr / fund. -- "; }
                    a_start_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_start_date; text="start date"; type="editbox"; form=a_payroll_item_import_form; tooltip="starting date that this payroll item record applies to -- "; }
                    }
                a_payroll_item_import_hbox6 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_end_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_end_date; text="end date"; type="editbox"; form=a_payroll_item_import_form; tooltip="ending date that this payroll item record applies to -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=a_payroll_item_import_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=a_payroll_item_import_form; tooltip=" -- "; }
                    }
                a_payroll_item_import_hbox7 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=a_payroll_item_import_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=a_payroll_item_import_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=a_payroll_item_import_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
