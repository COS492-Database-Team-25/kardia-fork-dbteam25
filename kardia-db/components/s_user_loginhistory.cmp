$Version=2$
s_user_loginhistory_component "widget/component-decl"
    {
    width=835;height=635;
    s_user_loginhistory_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :s_date_modified, :s_sessionid, :s_created_by, :__cx_osml_control, :s_last_seen, :s_first_seen, :s_date_created, :s_username FROM /lightsys/partner_DB/s_user_loginhistory/rows";
        baseobj = "/lightsys/partner_DB/s_user_loginhistory/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        s_user_loginhistory_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            s_user_loginhistory_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            s_user_loginhistory_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                s_user_loginhistory_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_username_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_username; text="username"; type="editbox"; form=s_user_loginhistory_form; tooltip="user who logged in. -- "; }
                    s_sessionid_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_sessionid; text="sessionid"; type="editbox"; form=s_user_loginhistory_form; tooltip="an incrementing value for this particular login record. -- "; }
                    s_first_seen_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_first_seen; text="first seen"; type="editbox"; form=s_user_loginhistory_form; tooltip="this login session began on this date/time -- "; }
                    }
                s_user_loginhistory_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_last_seen_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_last_seen; text="last seen"; type="editbox"; form=s_user_loginhistory_form; tooltip="this login session ended on or around this date/time -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=s_user_loginhistory_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=s_user_loginhistory_form; tooltip=" -- "; }
                    }
                s_user_loginhistory_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=s_user_loginhistory_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=s_user_loginhistory_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=s_user_loginhistory_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
