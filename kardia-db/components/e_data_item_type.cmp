$Version=2$
e_data_item_type_component "widget/component-decl"
    {
    width=835;height=635;
    e_data_item_type_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :e_data_item_type_highlight_if, :s_date_created, :e_parent_data_item_type_id, :e_data_item_type_type, :e_data_item_type_id, :e_data_item_type_label, :s_date_modified, :s_created_by, :__cx_osml_control, :e_data_item_type_desc, :e_data_item_type_highlight FROM /lightsys/partner_DB/e_data_item_type/rows";
        baseobj = "/lightsys/partner_DB/e_data_item_type/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        e_data_item_type_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            e_data_item_type_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            e_data_item_type_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                e_data_item_type_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_data_item_type_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_data_item_type_id; text="data item type id"; type="editbox"; form=e_data_item_type_form; tooltip="The data item type ID -- "; }
                    e_parent_data_item_type_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_parent_data_item_type_id; text="parent data item type id"; type="editbox"; form=e_data_item_type_form; tooltip="The parent data item type ID (hierarchy reference) -- "; }
                    e_data_item_type_label_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_data_item_type_label; text="data item type label"; type="editbox"; form=e_data_item_type_form; tooltip="A short data item type label -- "; }
                    }
                e_data_item_type_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_data_item_type_desc_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_data_item_type_desc; text="data item type desc"; type="editbox"; form=e_data_item_type_form; tooltip="A description of this data item type -- "; }
                    e_data_item_type_type_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_data_item_type_type; text="data item type type"; type="editbox"; form=e_data_item_type_form; tooltip="Data type: string, integer, double, datetime, money -- "; }
                    e_data_item_type_highlight_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_data_item_type_highlight; text="data item type highlight"; type="editbox"; form=e_data_item_type_form; tooltip="Set to 0 or null to not highlight this item, or 1 to highlight it on the profile page -- "; }
                    }
                e_data_item_type_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_data_item_type_highlight_if_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_data_item_type_highlight_if; text="data item type highlight if"; type="editbox"; form=e_data_item_type_form; tooltip="If set, this is compared with the data item value and highlighted IF they match. -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=e_data_item_type_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=e_data_item_type_form; tooltip=" -- "; }
                    }
                e_data_item_type_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=e_data_item_type_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=e_data_item_type_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=e_data_item_type_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
