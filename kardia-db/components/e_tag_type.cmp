$Version=2$
e_tag_type_component "widget/component-decl"
    {
    width=835;height=635;
    e_tag_type_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :e_is_active, :e_tag_threshold, :e_tag_id, :s_date_created, :e_tag_desc, :s_date_modified, :s_created_by, :__cx_osml_control, :e_tag_label FROM /lightsys/partner_DB/e_tag_type/rows";
        baseobj = "/lightsys/partner_DB/e_tag_type/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        e_tag_type_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            e_tag_type_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            e_tag_type_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                e_tag_type_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_tag_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_tag_id; text="tag id"; type="editbox"; form=e_tag_type_form; tooltip="ID of the tag -- "; }
                    e_tag_label_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_tag_label; text="tag label"; type="editbox"; form=e_tag_type_form; tooltip="Short label for this tag. -- "; }
                    e_tag_desc_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_tag_desc; text="tag desc"; type="editbox"; form=e_tag_type_form; tooltip="A full description of the tag. -- "; }
                    }
                e_tag_type_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_is_active_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_is_active; text="is active"; type="editbox"; form=e_tag_type_form; tooltip="1 if active, or 0 if not active. An inactive tag may still be present in the tag graph, but will not be available to the user to select. -- "; }
                    e_tag_threshold_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_tag_threshold; text="tag threshold"; type="editbox"; form=e_tag_type_form; tooltip="A value between 0.0 and 1.0 that indicates how strong a tag must be in order for it to actually be created in the tag graph for a partner. -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=e_tag_type_form; tooltip=" -- "; }
                    }
                e_tag_type_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=e_tag_type_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=e_tag_type_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=e_tag_type_form; tooltip=" -- "; }
                    }
                e_tag_type_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=e_tag_type_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
