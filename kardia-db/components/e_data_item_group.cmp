$Version=2$
e_data_item_group_component "widget/component-decl"
    {
    width=835;height=635;
    e_data_item_group_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :e_data_item_group_id, :s_date_created, :e_data_item_type_id, :s_date_modified, :e_data_item_group_name, :p_partner_key, :s_created_by, :__cx_osml_control, :e_data_item_group_desc FROM /lightsys/partner_DB/e_data_item_group/rows";
        baseobj = "/lightsys/partner_DB/e_data_item_group/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        e_data_item_group_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            e_data_item_group_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            e_data_item_group_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                e_data_item_group_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_data_item_group_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_data_item_group_id; text="data item group id"; type="editbox"; form=e_data_item_group_form; tooltip="The data item group ID -- "; }
                    e_data_item_type_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_data_item_type_id; text="data item type id"; type="editbox"; form=e_data_item_group_form; tooltip="The data item type hierarchy that this group relates to -- "; }
                    e_data_item_group_name_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_data_item_group_name; text="data item group name"; type="editbox"; form=e_data_item_group_form; tooltip="The name of this actual group -- "; }
                    }
                e_data_item_group_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_data_item_group_desc_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_data_item_group_desc; text="data item group desc"; type="editbox"; form=e_data_item_group_form; tooltip="A description of this group -- "; }
                    p_partner_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_partner_key; text="partner key"; type="editbox"; form=e_data_item_group_form; tooltip="The engaging partner that this data item group is about -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=e_data_item_group_form; tooltip=" -- "; }
                    }
                e_data_item_group_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=e_data_item_group_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=e_data_item_group_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=e_data_item_group_form; tooltip=" -- "; }
                    }
                e_data_item_group_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=e_data_item_group_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
