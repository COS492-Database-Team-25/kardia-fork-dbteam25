$Version=2$
e_trackactivity_component "widget/component-decl"
    {
    width=835;height=635;
    e_trackactivity_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :e_object_comm, :e_object_name, :e_step_id, :e_object_desc, :e_track_id, :s_date_created, :e_object_label, :s_date_modified, :e_username, :e_sort_key, :p_partner_key, :s_created_by, :__cx_osml_control, :e_object_type, :e_completion_status FROM /lightsys/partner_DB/e_trackactivity/rows";
        baseobj = "/lightsys/partner_DB/e_trackactivity/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        e_trackactivity_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            e_trackactivity_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            e_trackactivity_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                e_trackactivity_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_partner_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_partner_key; text="partner key"; type="editbox"; form=e_trackactivity_form; tooltip="The partner whose track information we are displaying -- "; }
                    e_username_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_username; text="username"; type="editbox"; form=e_trackactivity_form; tooltip="The username of the user viewing the partner -- "; }
                    e_sort_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_sort_key; text="sort key"; type="editbox"; form=e_trackactivity_form; tooltip="Controls the display order (steps by sequence, requirements by step sequence and req ID) -- "; }
                    }
                e_trackactivity_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_track_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_track_id; text="track id"; type="editbox"; form=e_trackactivity_form; tooltip="The ID of the track being displayed -- "; }
                    e_step_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_step_id; text="step id"; type="editbox"; form=e_trackactivity_form; tooltip="The ID of the step being displayed -- "; }
                    e_object_type_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_object_type; text="object type"; type="editbox"; form=e_trackactivity_form; tooltip="'S' for step and 'R' for requirement -- "; }
                    }
                e_trackactivity_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_completion_status_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_completion_status; text="completion status"; type="editbox"; form=e_trackactivity_form; tooltip="I/C/E for steps, I/C/W for requirements -- "; }
                    e_object_name_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_object_name; text="object name"; type="editbox"; form=e_trackactivity_form; tooltip="The "name" of the step or requirement being displayed (for linkage to a data editing form) -- "; }
                    e_object_label_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_object_label; text="object label"; type="editbox"; form=e_trackactivity_form; tooltip="The main visual label for the step or requirement -- "; }
                    }
                e_trackactivity_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_object_desc_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_object_desc; text="object desc"; type="editbox"; form=e_trackactivity_form; tooltip="The brief description of the step or requirement -- "; }
                    e_object_comm_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_object_comm; text="object comm"; type="editbox"; form=e_trackactivity_form; tooltip="Any comments associated with the step or requirement -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=e_trackactivity_form; tooltip=" -- "; }
                    }
                e_trackactivity_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=e_trackactivity_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=e_trackactivity_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=e_trackactivity_form; tooltip=" -- "; }
                    }
                e_trackactivity_hbox5 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=e_trackactivity_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
