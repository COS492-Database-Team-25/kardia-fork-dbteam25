$Version=2$
p_country_component "widget/component-decl"
    {
    width=835;height=635;
    p_country_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :p_fips_code, :p_country_name, :p_late_timezone, :s_date_created, :p_iso3166_2_code, :s_date_modified, :p_early_timezone, :p_record_status_code, :p_country_code, :p_iso3166_3_code, :p_phone_code, :p_nationality, :s_created_by, :p_local_name, :__cx_osml_control, :p_security_level FROM /lightsys/partner_DB/p_country/rows";
        baseobj = "/lightsys/partner_DB/p_country/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        p_country_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            p_country_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            p_country_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                p_country_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_country_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_country_code; text="country code"; type="editbox"; form=p_country_form; tooltip="ISO 3166-1 Alpha-2 With UK exception (basically ccTLD codes) -- "; }
                    p_iso3166_2_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_iso3166_2_code; text="iso3166 2 code"; type="editbox"; form=p_country_form; tooltip="ISO 3166-1 Alpha-2 -- "; }
                    p_iso3166_3_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_iso3166_3_code; text="iso3166 3 code"; type="editbox"; form=p_country_form; tooltip="ISO 3166-1 Alpha-3 -- "; }
                    }
                p_country_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_fips_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_fips_code; text="fips code"; type="editbox"; form=p_country_form; tooltip="Two-letter codes used by FIPS 10-4 / CIA World Factbook / Joshua Project / HIS-ROG3 -- "; }
                    p_country_name_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_country_name; text="country name"; type="editbox"; form=p_country_form; tooltip="The name of the country -- "; }
                    p_local_name_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_local_name; text="local name"; type="editbox"; form=p_country_form; tooltip="The localized name of the country -- "; }
                    }
                p_country_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_phone_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_phone_code; text="phone code"; type="editbox"; form=p_country_form; tooltip="The phone calling code for this country -- "; }
                    p_security_level_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_security_level; text="security level"; type="editbox"; form=p_country_form; tooltip="The security level for this country (from Joshua project) -- "; }
                    p_nationality_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_nationality; text="nationality"; type="editbox"; form=p_country_form; tooltip="Nationality of the people -- "; }
                    }
                p_country_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_early_timezone_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_early_timezone; text="early timezone"; type="editbox"; form=p_country_form; tooltip="Earliest timezone in the country -- "; }
                    p_late_timezone_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_late_timezone; text="late timezone"; type="editbox"; form=p_country_form; tooltip="Latest timezone in the country -- "; }
                    p_record_status_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_record_status_code; text="record status code"; type="editbox"; form=p_country_form; tooltip="(A)ctive, (O)bsolete -- for listing old addresses that have obsolete country codes. -- "; }
                    }
                p_country_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=p_country_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=p_country_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=p_country_form; tooltip=" -- "; }
                    }
                p_country_hbox5 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=p_country_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=p_country_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
