$Version=2$
a_subtrx_gift_component "widget/component-decl"
    {
    width=835;height=635;
    a_subtrx_gift_osrc "widget/osrc"
        {
        sql="SELECT :a_amount, :a_receipt_desired, :a_calc_admin_fee, :a_calc_admin_fee_subtype, :a_gift_type, :a_batch_number, :a_cost_center, :a_receipt_sent, :a_first_gift, :a_receipt_number, :a_gift_received_date, :s_date_created, :a_calc_admin_fee_type, :a_posted_to_gl, :a_gift_postmark_date, :a_posted, :a_recv_document_id, :a_receipt_sent_date, :a_gift_number, :a_goods_provided, :p_recip_partner_id, :a_gift_admin_fee, :a_period, :__cx_osml_control, :p_donor_partner_id, :a_comment, :a_ledger_number, :a_anonymous_gift, :a_personal_gift, :s_modified_by, :a_account_code, :s_date_modified, :a_gift_admin_subtype, :s_created_by FROM /lightsys/partner_DB/a_subtrx_gift/rows";
        baseobj = "/lightsys/partner_DB/a_subtrx_gift/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        a_subtrx_gift_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            a_subtrx_gift_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            a_subtrx_gift_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                a_subtrx_gift_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_ledger_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_ledger_number; text="ledger number"; type="editbox"; form=a_subtrx_gift_form; tooltip="ledger number for this gift. -- "; }
                    a_batch_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_batch_number; text="batch number"; type="editbox"; form=a_subtrx_gift_form; tooltip="Batch id for this gift. -- "; }
                    a_gift_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_gift_number; text="gift number"; type="editbox"; form=a_subtrx_gift_form; tooltip="sequential gift number in the batch. -- "; }
                    }
                a_subtrx_gift_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_period_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_period; text="period"; type="editbox"; form=a_subtrx_gift_form; tooltip="Accounting period this transaction is recorded in. -- "; }
                    a_cost_center_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_cost_center; text="cost center"; type="editbox"; form=a_subtrx_gift_form; tooltip="Which fund the gift is given to. -- "; }
                    a_account_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_account_code; text="account code"; type="editbox"; form=a_subtrx_gift_form; tooltip="Which GL Account this gift posts to in the above fund. -- "; }
                    }
                a_subtrx_gift_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_amount_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_amount; text="amount"; type="editbox"; form=a_subtrx_gift_form; tooltip="Amount of the gift. -- "; }
                    a_posted_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_posted; text="posted"; type="editbox"; form=a_subtrx_gift_form; tooltip="Has this transaction been posted (in this table)? -- "; }
                    a_posted_to_gl_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_posted_to_gl; text="posted to gl"; type="editbox"; form=a_subtrx_gift_form; tooltip="Has this transaction been posted to the GL - yes (1) or no (0)? -- "; }
                    }
                a_subtrx_gift_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_gift_type_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_gift_type; text="gift type"; type="editbox"; form=a_subtrx_gift_form; tooltip="Type of gift: (C)ash, chec(K), credit car(D), (E)FT -- "; }
                    a_gift_admin_fee_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_gift_admin_fee; text="gift admin fee"; type="editbox"; form=a_subtrx_gift_form; tooltip="Total administration fee percent to use (optionally specified by user) -- "; }
                    a_gift_admin_subtype_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_gift_admin_subtype; text="gift admin subtype"; type="editbox"; form=a_subtrx_gift_form; tooltip="Admin fee subtype to use (overrides that specified in a_cc_admin_fee) -- "; }
                    }
                a_subtrx_gift_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_calc_admin_fee_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_calc_admin_fee; text="calc admin fee"; type="editbox"; form=a_subtrx_gift_form; tooltip="Total administration fee percent as calculated by the system (set by admin fee logic) -- "; }
                    a_calc_admin_fee_type_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_calc_admin_fee_type; text="calc admin fee type"; type="editbox"; form=a_subtrx_gift_form; tooltip="admin fee type as calculated by the system (set by admin fee logic) -- "; }
                    a_calc_admin_fee_subtype_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_calc_admin_fee_subtype; text="calc admin fee subtype"; type="editbox"; form=a_subtrx_gift_form; tooltip="admin fee subtype as calculated by the system (set by admin fee logic) -- "; }
                    }
                a_subtrx_gift_hbox5 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_recv_document_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_recv_document_id; text="recv document id"; type="editbox"; form=a_subtrx_gift_form; tooltip="Check number, transaction number, etc., for received gift. -- "; }
                    a_receipt_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_receipt_number; text="receipt number"; type="editbox"; form=a_subtrx_gift_form; tooltip="Receipt number that we sent out -- "; }
                    p_donor_partner_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_donor_partner_id; text="donor partner id"; type="editbox"; form=a_subtrx_gift_form; tooltip="Partner ID of donor -- "; }
                    }
                a_subtrx_gift_hbox6 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_recip_partner_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_recip_partner_id; text="recip partner id"; type="editbox"; form=a_subtrx_gift_form; tooltip="Partner ID of gift recipient. May not be needed for non-pool based financial setups. -- "; }
                    a_receipt_sent_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_receipt_sent; text="receipt sent"; type="editbox"; form=a_subtrx_gift_form; tooltip="Receipt sent to donor -- "; }
                    a_receipt_desired_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_receipt_desired; text="receipt desired"; type="editbox"; form=a_subtrx_gift_form; tooltip="Receipt needed -- should be "1" for all gifts unless otherwise specified -- "; }
                    }
                a_subtrx_gift_hbox7 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_anonymous_gift_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_anonymous_gift; text="anonymous gift"; type="editbox"; form=a_subtrx_gift_form; tooltip="Set this if the donor wishes to remain anonymous (to the recipient) -- "; }
                    a_personal_gift_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_personal_gift; text="personal gift"; type="editbox"; form=a_subtrx_gift_form; tooltip="Set this if the gift is a personal gift (i.e., payable to missionary instead of support gift) -- "; }
                    a_first_gift_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_first_gift; text="first gift"; type="editbox"; form=a_subtrx_gift_form; tooltip="Donor's first gift? -- "; }
                    }
                a_subtrx_gift_hbox8 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_goods_provided_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_goods_provided; text="goods provided"; type="editbox"; form=a_subtrx_gift_form; tooltip="Amount of gift that is considered a fee for goods provided in return -- "; }
                    a_gift_received_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_gift_received_date; text="gift received date"; type="editbox"; form=a_subtrx_gift_form; tooltip="Date a gift actually was received by the office -- "; }
                    a_gift_postmark_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_gift_postmark_date; text="gift postmark date"; type="editbox"; form=a_subtrx_gift_form; tooltip="Date a gift was postmarked (if available) -- "; }
                    }
                a_subtrx_gift_hbox9 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_receipt_sent_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_receipt_sent_date; text="receipt sent date"; type="editbox"; form=a_subtrx_gift_form; tooltip="Date the receipt was sent to the donor -- "; }
                    a_comment_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_comment; text="comment"; type="editbox"; form=a_subtrx_gift_form; tooltip="Gift comments -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=a_subtrx_gift_form; tooltip=" -- "; }
                    }
                a_subtrx_gift_hbox10 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=a_subtrx_gift_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=a_subtrx_gift_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=a_subtrx_gift_form; tooltip=" -- "; }
                    }
                a_subtrx_gift_hbox11 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=a_subtrx_gift_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
