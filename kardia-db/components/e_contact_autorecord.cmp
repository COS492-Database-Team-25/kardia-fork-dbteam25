$Version=2$
e_contact_autorecord_component "widget/component-decl"
    {
    width=835;height=635;
    e_contact_autorecord_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :e_collaborator_id, :e_auto_record_apply_all, :s_date_created, :e_auto_record, :e_contact_history_type, :s_date_modified, :p_partner_key, :s_created_by, :e_contact_id, :__cx_osml_control, :e_engagement_id, :e_comments FROM /lightsys/partner_DB/e_contact_autorecord/rows";
        baseobj = "/lightsys/partner_DB/e_contact_autorecord/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        e_contact_autorecord_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            e_contact_autorecord_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            e_contact_autorecord_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                e_contact_autorecord_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_partner_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_partner_key; text="partner key"; type="editbox"; form=e_contact_autorecord_form; tooltip="The partner ID that is doing the engaging -- "; }
                    e_collaborator_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_collaborator_id; text="collaborator id"; type="editbox"; form=e_contact_autorecord_form; tooltip="The collaborator that the partner communicated with -- "; }
                    e_contact_history_type_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_contact_history_type; text="contact history type"; type="editbox"; form=e_contact_autorecord_form; tooltip="The type of the contact to automatically record (email / phone / etc), or "ALL" to apply to all types -- "; }
                    }
                e_contact_autorecord_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_contact_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_contact_id; text="contact id"; type="editbox"; form=e_contact_autorecord_form; tooltip="The contact info id (p_contact_info) to auto record, or (-1) to apply to ALL contact records relevant to the history type above. -- "; }
                    e_engagement_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_engagement_id; text="engagement id"; type="editbox"; form=e_contact_autorecord_form; tooltip="The engagement that this contact is in regard to (null if general or pre-engagement) -- "; }
                    e_auto_record_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_auto_record; text="auto record"; type="editbox"; form=e_contact_autorecord_form; tooltip="Whether to auto-record: 1=yes, 0=no -- "; }
                    }
                e_contact_autorecord_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_auto_record_apply_all_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_auto_record_apply_all; text="auto record apply all"; type="editbox"; form=e_contact_autorecord_form; tooltip="Whether this auto-record record applies to just this collaborator (0) or to ALL collaborators (1). The auto_record setting for a specific collaborator will override one set for all collaborators. If more than one 'all collaborators' record is present and they conflict, then the default is ON (1), to automatically record the contacts. -- "; }
                    e_comments_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_comments; text="comments"; type="editbox"; form=e_contact_autorecord_form; tooltip="User-entered comments on this engagement track/step for this partner -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=e_contact_autorecord_form; tooltip=" -- "; }
                    }
                e_contact_autorecord_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=e_contact_autorecord_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=e_contact_autorecord_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=e_contact_autorecord_form; tooltip=" -- "; }
                    }
                e_contact_autorecord_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=e_contact_autorecord_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
