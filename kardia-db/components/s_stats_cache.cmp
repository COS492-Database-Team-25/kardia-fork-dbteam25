$Version=2$
s_stats_cache_component "widget/component-decl"
    {
    width=835;height=635;
    s_stats_cache_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :s_string_value, :s_stat_group, :s_date_created, :s_double_value, :s_stat, :s_datetime_value, :s_date_modified, :s_stat_type, :s_created_by, :__cx_osml_control, :s_money_value, :s_integer_value FROM /lightsys/partner_DB/s_stats_cache/rows";
        baseobj = "/lightsys/partner_DB/s_stats_cache/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        s_stats_cache_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            s_stats_cache_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            s_stats_cache_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                s_stats_cache_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_stat_type_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_stat_type; text="stat type"; type="editbox"; form=s_stats_cache_form; tooltip="unique identifier of the statistic type -- "; }
                    s_stat_group_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_stat_group; text="stat group"; type="editbox"; form=s_stats_cache_form; tooltip="subgroup within the statistic type -- "; }
                    s_stat_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_stat; text="stat"; type="editbox"; form=s_stats_cache_form; tooltip="identifier of this particular statistic -- "; }
                    }
                s_stats_cache_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_string_value_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_string_value; text="string value"; type="editbox"; form=s_stats_cache_form; tooltip="string value of the statistic -- "; }
                    s_integer_value_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_integer_value; text="integer value"; type="editbox"; form=s_stats_cache_form; tooltip="integer value of the statistic -- "; }
                    s_money_value_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_money_value; text="money value"; type="editbox"; form=s_stats_cache_form; tooltip="currency value of the statistic -- "; }
                    }
                s_stats_cache_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_double_value_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_double_value; text="double value"; type="editbox"; form=s_stats_cache_form; tooltip="floating point value of the statistic -- "; }
                    s_datetime_value_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_datetime_value; text="datetime value"; type="editbox"; form=s_stats_cache_form; tooltip="date/time value of the statistic -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=s_stats_cache_form; tooltip=" -- "; }
                    }
                s_stats_cache_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=s_stats_cache_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=s_stats_cache_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=s_stats_cache_form; tooltip=" -- "; }
                    }
                s_stats_cache_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=s_stats_cache_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
