$Version=2$
p_bulk_postal_code_component "widget/component-decl"
    {
    width=835;height=635;
    p_bulk_postal_code_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :p_zip_state, :p_bulk_code, :s_date_created, :s_date_modified, :p_country_code, :s_created_by, :__cx_osml_control, :p_bulk_postal_code, :p_bulk_postal_code_description, :p_bulk_range FROM /lightsys/partner_DB/p_bulk_postal_code/rows";
        baseobj = "/lightsys/partner_DB/p_bulk_postal_code/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        p_bulk_postal_code_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            p_bulk_postal_code_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            p_bulk_postal_code_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                p_bulk_postal_code_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_bulk_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_bulk_code; text="bulk code"; type="editbox"; form=p_bulk_postal_code_form; tooltip="The Post-office code for the bulk mailing type, for example, L801 -- "; }
                    p_country_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_country_code; text="country code"; type="editbox"; form=p_bulk_postal_code_form; tooltip="The country the code is for -- "; }
                    p_bulk_postal_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_bulk_postal_code; text="bulk postal code"; type="editbox"; form=p_bulk_postal_code_form; tooltip="The Zip Center (3 digits) -- "; }
                    }
                p_bulk_postal_code_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_bulk_postal_code_description_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_bulk_postal_code_description; text="bulk postal code description"; type="editbox"; form=p_bulk_postal_code_form; tooltip="Name of zip center (city, etc) -- "; }
                    p_zip_state_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_zip_state; text="zip state"; type="editbox"; form=p_bulk_postal_code_form; tooltip="The State the zip is in -- "; }
                    p_bulk_range_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_bulk_range; text="bulk range"; type="editbox"; form=p_bulk_postal_code_form; tooltip="The range that the zipcenter falls into. Used for double-checking purposes only -- "; }
                    }
                p_bulk_postal_code_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=p_bulk_postal_code_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=p_bulk_postal_code_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=p_bulk_postal_code_form; tooltip=" -- "; }
                    }
                p_bulk_postal_code_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=p_bulk_postal_code_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=p_bulk_postal_code_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
