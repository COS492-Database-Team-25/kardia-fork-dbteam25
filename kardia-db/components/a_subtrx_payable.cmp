                    a_taxable_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_taxable; text="taxable"; type="editbox"; form=a_subtrx_payable_form; tooltip="Whether this is a taxable payable (0/1) -- "; }
                    }
                a_subtrx_payable_hbox4 "widget/hbox"
