                    spacing=20;
                    width=750;
                    height=25;
                    a_advance_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_advance; text="advance"; type="editbox"; form=a_subtrx_payable_form; tooltip="Whether this is an expense advance (0/1) -- "; }
                    a_payment_terms_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_payment_terms; text="payment terms"; type="editbox"; form=a_subtrx_payable_form; tooltip="The terms on which the payment is required. Use "PAYROLL" to cause this to be disbursed with next payroll. -- "; }
                    a_payment_method_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_payment_method; text="payment method"; type="editbox"; form=a_subtrx_payable_form; tooltip="Requested payment method -- "; }
                    }
                a_subtrx_payable_hbox5 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_document_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_document_id; text="document id"; type="editbox"; form=a_subtrx_payable_form; tooltip="A document/image providing proof of this expense, if available, such as a receipt or invoice -- "; }
                    a_due_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_due_date; text="due date"; type="editbox"; form=a_subtrx_payable_form; tooltip="When the payable is due to be paid -- "; }
                    a_payee_partner_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_payee_partner_key; text="payee partner key"; type="editbox"; form=a_subtrx_payable_form; tooltip="Partner id of the payee (recipient) -- "; }
                    }
                a_subtrx_payable_hbox6 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_account_with_payee_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_account_with_payee; text="account with payee"; type="editbox"; form=a_subtrx_payable_form; tooltip="Account number at payee -- "; }
                    a_invoice_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_invoice_number; text="invoice number"; type="editbox"; form=a_subtrx_payable_form; tooltip="Invoice number associated with this payable -- "; }
                    a_make_payment_to_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_make_payment_to; text="make payment to"; type="editbox"; form=a_subtrx_payable_form; tooltip="Name to make the payment to, if different from payee name -- "; }
                    }
                a_subtrx_payable_hbox7 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_payment_comment_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_payment_comment; text="payment comment"; type="editbox"; form=a_subtrx_payable_form; tooltip="Comment to be included on the check or payment when it is sent -- "; }
                    a_comment_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_comment; text="comment"; type="editbox"; form=a_subtrx_payable_form; tooltip="Payable comments -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=a_subtrx_payable_form; tooltip=" -- "; }
                    }
                a_subtrx_payable_hbox8 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=a_subtrx_payable_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=a_subtrx_payable_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=a_subtrx_payable_form; tooltip=" -- "; }
                    }
                a_subtrx_payable_hbox9 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
