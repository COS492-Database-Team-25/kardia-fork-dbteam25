                    a_posted_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_posted; text="posted"; type="editbox"; form=a_subtrx_payable_form; tooltip="Whether the payable is posted or not (0/1) -- "; }
                    a_requested_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_requested_by; text="requested by"; type="editbox"; form=a_subtrx_payable_form; tooltip="Partner who requested the payable -- "; }
