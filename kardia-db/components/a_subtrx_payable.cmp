$Version=2$
a_subtrx_payable_component "widget/component-decl"
    {
    width=835;height=635;
    a_subtrx_payable_osrc "widget/osrc"
        {
        sql="SELECT :a_advance, :a_invoice_number, :a_payment_comment, :a_payment_method, :a_resolved_date, :a_resolved_by, :a_taxable, :a_payment_terms, :a_payable_id, :a_requested_date, :__cx_osml_control, :a_comment, :a_document_id, :s_modified_by, :a_ledger_number, :a_requested_by, :a_due_date, :s_date_created, :a_make_payment_to, :a_batch_number_resolution, :a_approved_by, :a_batch_number_payable, :a_account_with_payee, :s_date_modified, :a_approved_date, :a_posted, :s_created_by, :a_payee_partner_key FROM /lightsys/partner_DB/a_subtrx_payable/rows";
        baseobj = "/lightsys/partner_DB/a_subtrx_payable/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        a_subtrx_payable_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            a_subtrx_payable_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
