                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                a_subtrx_payable_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_ledger_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_ledger_number; text="ledger number"; type="editbox"; form=a_subtrx_payable_form; tooltip="ledger number for this disbursement -- "; }
                    a_payable_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_payable_id; text="payable id"; type="editbox"; form=a_subtrx_payable_form; tooltip="id number for this payable item -- "; }
                    a_batch_number_payable_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_batch_number_payable; text="batch number payable"; type="editbox"; form=a_subtrx_payable_form; tooltip="Batch id for the payable/liability journal -- "; }
                    }
