$Version=2$
p_contact_usage_type_component "widget/component-decl"
    {
    width=835;height=635;
    p_contact_usage_type_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :p_use_for_locations, :p_contact_types, :p_contact_usage_label, :s_date_created, :p_contact_usage_type_code, :s_date_modified, :p_use_for_contacts, :s_created_by, :__cx_osml_control, :p_system_provided FROM /lightsys/partner_DB/p_contact_usage_type/rows";
        baseobj = "/lightsys/partner_DB/p_contact_usage_type/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        p_contact_usage_type_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            p_contact_usage_type_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            p_contact_usage_type_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                p_contact_usage_type_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_contact_usage_type_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_contact_usage_type_code; text="contact usage type code"; type="editbox"; form=p_contact_usage_type_form; tooltip="Unique code for this usage type -- "; }
                    p_system_provided_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_system_provided; text="system provided"; type="editbox"; form=p_contact_usage_type_form; tooltip="1 = this code is provided with the system and cannot be changed or removed -- "; }
                    p_use_for_locations_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_use_for_locations; text="use for locations"; type="editbox"; form=p_contact_usage_type_form; tooltip="1 = this code can be used for p_location records. -- "; }
                    }
                p_contact_usage_type_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_use_for_contacts_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_use_for_contacts; text="use for contacts"; type="editbox"; form=p_contact_usage_type_form; tooltip="1 = this code can be used for contact data (phone, email, etc.) -- "; }
                    p_contact_types_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_contact_types; text="contact types"; type="editbox"; form=p_contact_usage_type_form; tooltip="a list of contact type codes that this code applies to. -- "; }
                    p_contact_usage_label_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_contact_usage_label; text="contact usage label"; type="editbox"; form=p_contact_usage_type_form; tooltip="The label/description for this usage code. -- "; }
                    }
                p_contact_usage_type_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=p_contact_usage_type_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=p_contact_usage_type_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=p_contact_usage_type_form; tooltip=" -- "; }
                    }
                p_contact_usage_type_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=p_contact_usage_type_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=p_contact_usage_type_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
