$Version=2$
h_staff_component "widget/component-decl"
    {
    width=835;height=635;
    h_staff_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :h_staff_role, :h_is_active, :s_date_created, :h_percent_fulltime, :s_date_modified, :p_partner_key, :s_created_by, :__cx_osml_control, :h_start_date FROM /lightsys/partner_DB/h_staff/rows";
        baseobj = "/lightsys/partner_DB/h_staff/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        h_staff_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            h_staff_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            h_staff_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                h_staff_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_partner_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_partner_key; text="partner key"; type="editbox"; form=h_staff_form; tooltip="The partner key of the staff member -- "; }
                    h_is_active_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=h_is_active; text="is active"; type="editbox"; form=h_staff_form; tooltip="Set to 0 to disable this partner as a staff member. -- "; }
                    h_start_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=h_start_date; text="start date"; type="editbox"; form=h_staff_form; tooltip="Original starting date for this staff member. -- "; }
                    }
                h_staff_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    h_staff_role_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=h_staff_role; text="staff role"; type="editbox"; form=h_staff_form; tooltip="Role of this staff member: (E)mployee, (V)olunteer, (N)one -- "; }
                    h_percent_fulltime_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=h_percent_fulltime; text="percent fulltime"; type="editbox"; form=h_staff_form; tooltip="Percent of full-time status. -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=h_staff_form; tooltip=" -- "; }
                    }
                h_staff_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=h_staff_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=h_staff_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=h_staff_form; tooltip=" -- "; }
                    }
                h_staff_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=h_staff_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
