$Version=2$
e_todo_component "widget/component-decl"
    {
    width=835;height=635;
    e_todo_osrc "widget/osrc"
        {
        sql="SELECT :e_todo_req_item_id, :e_todo_partner, :e_todo_engagement_id, :e_todo_type_id, :e_todo_document_id, :e_todo_collaborator, :e_todo_completion_date, :__cx_osml_control, :e_todo_status, :e_todo_canceled_date, :e_todo_due_date, :s_modified_by, :e_todo_id, :e_todo_desc, :s_date_created, :s_date_modified, :s_created_by, :e_todo_comments FROM /lightsys/partner_DB/e_todo/rows";
        baseobj = "/lightsys/partner_DB/e_todo/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        e_todo_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            e_todo_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            e_todo_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                e_todo_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_todo_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_todo_id; text="todo id"; type="editbox"; form=e_todo_form; tooltip="The todo ID -- "; }
                    e_todo_type_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_todo_type_id; text="todo type id"; type="editbox"; form=e_todo_form; tooltip="The todo type ID -- "; }
                    e_todo_desc_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_todo_desc; text="todo desc"; type="editbox"; form=e_todo_form; tooltip="The description of this todo -- "; }
                    }
                e_todo_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_todo_comments_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_todo_comments; text="todo comments"; type="editbox"; form=e_todo_form; tooltip="Misc comments about this todo item -- "; }
                    e_todo_status_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_todo_status; text="todo status"; type="editbox"; form=e_todo_form; tooltip="To-do item status: (I)ncomplete, (C)omplete, or (X) for canceled. -- "; }
                    e_todo_completion_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_todo_completion_date; text="todo completion date"; type="editbox"; form=e_todo_form; tooltip="Completion date for a To-do that has been completed. -- "; }
                    }
                e_todo_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_todo_canceled_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_todo_canceled_date; text="todo canceled date"; type="editbox"; form=e_todo_form; tooltip="Cancellation date for a To-do that has been canceled. -- "; }
                    e_todo_due_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_todo_due_date; text="todo due date"; type="editbox"; form=e_todo_form; tooltip="Due date for this to-do item -- "; }
                    e_todo_collaborator_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_todo_collaborator; text="todo collaborator"; type="editbox"; form=e_todo_form; tooltip="The collaborator that needs to do this todo item -- "; }
                    }
                e_todo_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_todo_partner_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_todo_partner; text="todo partner"; type="editbox"; form=e_todo_form; tooltip="The engaging partner that this todo relates to -- "; }
                    e_todo_engagement_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_todo_engagement_id; text="todo engagement id"; type="editbox"; form=e_todo_form; tooltip="The engagement ID, if any, that this todo relates to -- "; }
                    e_todo_document_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_todo_document_id; text="todo document id"; type="editbox"; form=e_todo_form; tooltip="The document ID, if any, that this todo relates to -- "; }
                    }
                e_todo_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_todo_req_item_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_todo_req_item_id; text="todo req item id"; type="editbox"; form=e_todo_form; tooltip="The requirement item ID, if any, that this todo relates to -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=e_todo_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=e_todo_form; tooltip=" -- "; }
                    }
                e_todo_hbox5 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=e_todo_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=e_todo_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=e_todo_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
