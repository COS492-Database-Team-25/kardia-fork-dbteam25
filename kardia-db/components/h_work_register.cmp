$Version=2$
h_work_register_component "widget/component-decl"
    {
    width=835;height=635;
    h_work_register_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :h_work_type, :h_work_date, :s_date_created, :s_date_modified, :p_partner_key, :s_created_by, :__cx_osml_control, :h_work_register_id, :h_benefit_type_id, :h_work_hours FROM /lightsys/partner_DB/h_work_register/rows";
        baseobj = "/lightsys/partner_DB/h_work_register/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        h_work_register_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            h_work_register_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            h_work_register_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                h_work_register_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_partner_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_partner_key; text="partner key"; type="editbox"; form=h_work_register_form; tooltip="The partner key of the employee -- "; }
                    h_work_register_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=h_work_register_id; text="work register id"; type="editbox"; form=h_work_register_form; tooltip="Unique ID of the work register entry for this partner. -- "; }
                    h_work_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=h_work_date; text="work date"; type="editbox"; form=h_work_register_form; tooltip="Date that this work record applies to (date only, work times are in h_work_register_times) -- "; }
                    }
                h_work_register_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    h_work_hours_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=h_work_hours; text="work hours"; type="editbox"; form=h_work_register_form; tooltip="Number of hours the employee worked. -- "; }
                    h_work_type_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=h_work_type; text="work type"; type="editbox"; form=h_work_register_form; tooltip="Type of work register entry: (N)ormal work, (T)imeoff used, (H)oliday used, (W)orked during a paid holiday -- "; }
                    h_benefit_type_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=h_benefit_type_id; text="benefit type id"; type="editbox"; form=h_work_register_form; tooltip="If paid time off was used ('T', above), this is the h_benefit_type of the paid time off. -- "; }
                    }
                h_work_register_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=h_work_register_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=h_work_register_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=h_work_register_form; tooltip=" -- "; }
                    }
                h_work_register_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=h_work_register_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=h_work_register_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
