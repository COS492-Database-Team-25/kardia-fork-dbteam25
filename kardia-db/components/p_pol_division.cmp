$Version=2$
p_pol_division_component "widget/component-decl"
    {
    width=835;height=635;
    p_pol_division_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :p_pol_division_name, :s_date_created, :p_pol_division, :s_date_modified, :p_country_code, :p_record_status_code, :p_local_name, :s_created_by, :__cx_osml_control, :p_pol_division_parent FROM /lightsys/partner_DB/p_pol_division/rows";
        baseobj = "/lightsys/partner_DB/p_pol_division/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        p_pol_division_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            p_pol_division_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            p_pol_division_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                p_pol_division_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_country_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_country_code; text="country code"; type="editbox"; form=p_pol_division_form; tooltip="ISO 3166-1 Alpha-2 With UK exception (basically ccTLD codes) -- "; }
                    p_pol_division_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_pol_division; text="pol division"; type="editbox"; form=p_pol_division_form; tooltip="Political division abbrev/code, or full name if no abbrev/code. -- "; }
                    p_pol_division_parent_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_pol_division_parent; text="pol division parent"; type="editbox"; form=p_pol_division_form; tooltip="Political division that this is a part of (null == top level state or province) -- "; }
                    }
                p_pol_division_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_pol_division_name_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_pol_division_name; text="pol division name"; type="editbox"; form=p_pol_division_form; tooltip="Political division name or label -- "; }
                    p_local_name_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_local_name; text="local name"; type="editbox"; form=p_pol_division_form; tooltip="The localized name of the political division -- "; }
                    p_record_status_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_record_status_code; text="record status code"; type="editbox"; form=p_pol_division_form; tooltip="(A)ctive, (O)bsolete -- for old addresses that have an obsolete state/province. -- "; }
                    }
                p_pol_division_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=p_pol_division_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=p_pol_division_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=p_pol_division_form; tooltip=" -- "; }
                    }
                p_pol_division_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=p_pol_division_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=p_pol_division_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
