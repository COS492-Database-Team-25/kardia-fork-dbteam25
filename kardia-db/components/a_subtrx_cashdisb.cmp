$Version=2$
a_subtrx_cashdisb_component "widget/component-decl"
    {
    width=835;height=635;
    a_subtrx_cashdisb_osrc "widget/osrc"
        {
        sql="SELECT :a_amount, :a_cash_account_code, :a_disbursement_id, :a_batch_number, :a_effective_date, :a_period, :a_cost_center, :__cx_osml_control, :a_comment, :s_modified_by, :a_ledger_number, :a_voided, :s_date_created, :a_account_code, :a_posted_to_gl, :s_date_modified, :a_posted, :s_created_by, :a_line_item, :a_payee_partner_key, :a_check_number FROM /lightsys/partner_DB/a_subtrx_cashdisb/rows";
        baseobj = "/lightsys/partner_DB/a_subtrx_cashdisb/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        a_subtrx_cashdisb_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            a_subtrx_cashdisb_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            a_subtrx_cashdisb_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                a_subtrx_cashdisb_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_ledger_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_ledger_number; text="ledger number"; type="editbox"; form=a_subtrx_cashdisb_form; tooltip="ledger number for this disbursement -- "; }
                    a_batch_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_batch_number; text="batch number"; type="editbox"; form=a_subtrx_cashdisb_form; tooltip="Batch id for this disbursement -- "; }
                    a_disbursement_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_disbursement_id; text="disbursement id"; type="editbox"; form=a_subtrx_cashdisb_form; tooltip="id number for this disbursement (check) -- "; }
                    }
                a_subtrx_cashdisb_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_line_item_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_line_item; text="line item"; type="editbox"; form=a_subtrx_cashdisb_form; tooltip="item number for the line item in the disbursement -- "; }
                    a_period_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_period; text="period"; type="editbox"; form=a_subtrx_cashdisb_form; tooltip="Accounting period this disbursement is recorded in. -- "; }
                    a_effective_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_effective_date; text="effective date"; type="editbox"; form=a_subtrx_cashdisb_form; tooltip="Effective date of disbursement (e.g., accrual date) -- "; }
                    }
                a_subtrx_cashdisb_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_cash_account_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_cash_account_code; text="cash account code"; type="editbox"; form=a_subtrx_cashdisb_form; tooltip="Cash account the funds are disbursed from -- "; }
                    a_amount_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_amount; text="amount"; type="editbox"; form=a_subtrx_cashdisb_form; tooltip="Amount of disbursement -- "; }
                    a_cost_center_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_cost_center; text="cost center"; type="editbox"; form=a_subtrx_cashdisb_form; tooltip="Cost center for the expense / liability side of the transaction -- "; }
                    }
                a_subtrx_cashdisb_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_account_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_account_code; text="account code"; type="editbox"; form=a_subtrx_cashdisb_form; tooltip="GL account for the expense / liability side of the transaction -- "; }
                    a_payee_partner_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_payee_partner_key; text="payee partner key"; type="editbox"; form=a_subtrx_cashdisb_form; tooltip="Partner id of the payee (recipient) -- "; }
                    a_check_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_check_number; text="check number"; type="editbox"; form=a_subtrx_cashdisb_form; tooltip="Check number being issued -- "; }
                    }
                a_subtrx_cashdisb_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_posted_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_posted; text="posted"; type="editbox"; form=a_subtrx_cashdisb_form; tooltip="Has this disbursement been posted (in this file) -- "; }
                    a_posted_to_gl_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_posted_to_gl; text="posted to gl"; type="editbox"; form=a_subtrx_cashdisb_form; tooltip="Has this disbursement been posted into the GL - yes (1) or no (0)? -- "; }
                    a_voided_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_voided; text="voided"; type="editbox"; form=a_subtrx_cashdisb_form; tooltip="Has this check been voided -- "; }
                    }
                a_subtrx_cashdisb_hbox5 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_comment_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_comment; text="comment"; type="editbox"; form=a_subtrx_cashdisb_form; tooltip="Xfer comments -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=a_subtrx_cashdisb_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=a_subtrx_cashdisb_form; tooltip=" -- "; }
                    }
                a_subtrx_cashdisb_hbox6 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=a_subtrx_cashdisb_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=a_subtrx_cashdisb_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=a_subtrx_cashdisb_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
