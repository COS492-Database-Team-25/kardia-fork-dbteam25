$Version=2$
c_member_component "widget/component-decl"
    {
    width=835;height=635;
    c_member_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :s_date_modified, :s_created_by, :c_status, :c_chat_id, :__cx_osml_control, :s_date_created, :c_last_read_message_id, :s_username FROM /lightsys/partner_DB/c_member/rows";
        baseobj = "/lightsys/partner_DB/c_member/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        c_member_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            c_member_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            c_member_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                c_member_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    c_chat_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=c_chat_id; text="chat id"; type="editbox"; form=c_member_form; tooltip="The key of the chat that the user has been invited to. -- "; }
                    s_username_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_username; text="username"; type="editbox"; form=c_member_form; tooltip="Foreign natural key of the user that has been invited to/is in the chat. -- "; }
                    c_last_read_message_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=c_last_read_message_id; text="last read message id"; type="editbox"; form=c_member_form; tooltip="The id of the last message that the user read from this chat. -- This could be null for invitations."; }
                    }
                c_member_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    c_status_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=c_status; text="status"; type="editbox"; form=c_member_form; tooltip="The user's status in the chat. 'I' is unanswered invitation, 'O' is in the chat (open chat), and 'C' is closed chat. This record is deleted if the user declines the invitation. -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=c_member_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=c_member_form; tooltip="Here, this is used for the person that invited them to the chat. -- "; }
                    }
                c_member_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=c_member_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=c_member_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=c_member_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
