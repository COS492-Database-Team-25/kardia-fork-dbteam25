$Version=2$
p_staff_component "widget/component-decl"
    {
    width=835;height=635;
    p_staff_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :s_date_created, :s_date_modified, :p_preferred_location_id, :p_partner_key, :p_is_staff, :s_created_by, :__cx_osml_control, :p_preferred_email_id, :p_kardia_login, :p_kardiaweb_login FROM /lightsys/partner_DB/p_staff/rows";
        baseobj = "/lightsys/partner_DB/p_staff/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        p_staff_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            p_staff_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            p_staff_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                p_staff_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_partner_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_partner_key; text="partner key"; type="editbox"; form=p_staff_form; tooltip="The partner key of the staff member -- "; }
                    p_is_staff_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_is_staff; text="is staff"; type="editbox"; form=p_staff_form; tooltip="Set to 0 to disable this partner as a staff member. -- "; }
                    p_kardia_login_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_kardia_login; text="kardia login"; type="editbox"; form=p_staff_form; tooltip="The Kardia login name of the staff member. -- "; }
                    }
                p_staff_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_kardiaweb_login_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_kardiaweb_login; text="kardiaweb login"; type="editbox"; form=p_staff_form; tooltip="The Kardia self-service website login of the staff member. -- "; }
                    p_preferred_email_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_preferred_email_id; text="preferred email id"; type="editbox"; form=p_staff_form; tooltip="Preferred email (p_contact_info) for receiving reports and such from Kardia -- "; }
                    p_preferred_location_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_preferred_location_id; text="preferred location id"; type="editbox"; form=p_staff_form; tooltip="Preferred location (p_location) for receiving reports and such from Kardia -- "; }
                    }
                p_staff_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=p_staff_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=p_staff_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=p_staff_form; tooltip=" -- "; }
                    }
                p_staff_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=p_staff_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=p_staff_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
