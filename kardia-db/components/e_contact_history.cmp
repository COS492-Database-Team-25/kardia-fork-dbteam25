$Version=2$
e_contact_history_component "widget/component-decl"
    {
    width=835;height=635;
    e_contact_history_osrc "widget/osrc"
        {
        sql="SELECT :e_contact_date, :e_contact_history_id, :e_contact_history_type, :p_partner_key, :__cx_osml_control, :p_location_revision_id, :p_location_partner_key, :e_whom, :e_initiation, :s_modified_by, :p_contact_id, :e_subject, :s_date_created, :e_message_id, :e_notes, :s_date_modified, :p_location_id, :s_created_by FROM /lightsys/partner_DB/e_contact_history/rows";
        baseobj = "/lightsys/partner_DB/e_contact_history/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        e_contact_history_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            e_contact_history_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            e_contact_history_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                e_contact_history_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_contact_history_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_contact_history_id; text="contact history id"; type="editbox"; form=e_contact_history_form; tooltip="The unique ID of this contact history item -- "; }
                    p_partner_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_partner_key; text="partner key"; type="editbox"; form=e_contact_history_form; tooltip="The partner key of the person that the organization contacted -- "; }
                    e_contact_history_type_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_contact_history_type; text="contact history type"; type="editbox"; form=e_contact_history_form; tooltip="The unique ID of this contact history type -- "; }
                    }
                e_contact_history_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_contact_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_contact_id; text="contact id"; type="editbox"; form=e_contact_history_form; tooltip="The p_contact_info ID that was used for this contact (such as a phone number, email address, etc.) -- "; }
                    p_location_partner_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_location_partner_key; text="location partner key"; type="editbox"; form=e_contact_history_form; tooltip="The partner key of the location where the contact took place (may be the same as the p_partner_key in some cases) -- "; }
                    p_location_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_location_id; text="location id"; type="editbox"; form=e_contact_history_form; tooltip="The p_location ID where the contact physically took place (for in-person contacts) -- "; }
                    }
                e_contact_history_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_location_revision_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_location_revision_id; text="location revision id"; type="editbox"; form=e_contact_history_form; tooltip="The p_location revision ID where the contact physically took place (for in-person contacts) -- "; }
                    e_whom_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_whom; text="whom"; type="editbox"; form=e_contact_history_form; tooltip="The collaborator/user who made the contact; NULL if no particular user was involved -- "; }
                    e_initiation_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_initiation; text="initiation"; type="editbox"; form=e_contact_history_form; tooltip="Set to 'P' if the engaging partner initiated the contact, or 'C' if the collaborator (organization) initiated the contact -- "; }
                    }
                e_contact_history_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_subject_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_subject; text="subject"; type="editbox"; form=e_contact_history_form; tooltip="A short description of the contact that took place -- "; }
                    e_contact_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_contact_date; text="contact date"; type="editbox"; form=e_contact_history_form; tooltip="The date and time that the contact took place -- "; }
                    e_notes_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_notes; text="notes"; type="editbox"; form=e_contact_history_form; tooltip="Notes regarding this contact -- "; }
                    }
                e_contact_history_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_message_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_message_id; text="message id"; type="editbox"; form=e_contact_history_form; tooltip="Message-ID header field of email message, or similar unique ID for other communications -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=e_contact_history_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=e_contact_history_form; tooltip=" -- "; }
                    }
                e_contact_history_hbox5 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=e_contact_history_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=e_contact_history_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=e_contact_history_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
