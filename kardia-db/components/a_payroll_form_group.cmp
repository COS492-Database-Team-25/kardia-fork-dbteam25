$Version=2$
a_payroll_form_group_component "widget/component-decl"
    {
    width=835;height=635;
    a_payroll_form_group_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :a_ledger_number, :a_payroll_form_desc, :s_date_created, :a_payroll_form_sequence, :s_date_modified, :a_payroll_item_type_code, :s_created_by, :__cx_osml_control, :a_payroll_form_group_name FROM /lightsys/partner_DB/a_payroll_form_group/rows";
        baseobj = "/lightsys/partner_DB/a_payroll_form_group/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        a_payroll_form_group_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            a_payroll_form_group_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            a_payroll_form_group_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                a_payroll_form_group_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_ledger_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_ledger_number; text="ledger number"; type="editbox"; form=a_payroll_form_group_form; tooltip="ledger number for the organization -- "; }
                    a_payroll_form_group_name_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_payroll_form_group_name; text="payroll form group name"; type="editbox"; form=a_payroll_form_group_form; tooltip="name for the group of payroll items -- "; }
                    a_payroll_form_sequence_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_payroll_form_sequence; text="payroll form sequence"; type="editbox"; form=a_payroll_form_group_form; tooltip="ascending numerical sequence to put the payroll form elements in -- "; }
                    }
                a_payroll_form_group_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_payroll_item_type_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_payroll_item_type_code; text="payroll item type code"; type="editbox"; form=a_payroll_form_group_form; tooltip="payroll item type code to display -- "; }
                    a_payroll_form_desc_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_payroll_form_desc; text="payroll form desc"; type="editbox"; form=a_payroll_form_group_form; tooltip="description to print next to the payroll form item -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=a_payroll_form_group_form; tooltip=" -- "; }
                    }
                a_payroll_form_group_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=a_payroll_form_group_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=a_payroll_form_group_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=a_payroll_form_group_form; tooltip=" -- "; }
                    }
                a_payroll_form_group_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=a_payroll_form_group_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
