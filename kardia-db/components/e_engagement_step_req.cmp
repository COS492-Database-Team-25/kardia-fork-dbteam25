$Version=2$
e_engagement_step_req_component "widget/component-decl"
    {
    width=835;height=635;
    e_engagement_step_req_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :e_step_id, :e_req_name, :e_track_id, :s_date_created, :e_due_days_from_req, :e_req_doc_type_id, :s_date_modified, :s_created_by, :e_req_id, :__cx_osml_control, :e_due_days_from_step, :e_due_days_from_req_id, :e_req_waivable, :e_req_whom FROM /lightsys/partner_DB/e_engagement_step_req/rows";
        baseobj = "/lightsys/partner_DB/e_engagement_step_req/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        e_engagement_step_req_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            e_engagement_step_req_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            e_engagement_step_req_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                e_engagement_step_req_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_track_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_track_id; text="track id"; type="editbox"; form=e_engagement_step_req_form; tooltip="A unique ID identifying the engagement track -- "; }
                    e_step_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_step_id; text="step id"; type="editbox"; form=e_engagement_step_req_form; tooltip="A unique ID identifying the engagement step -- "; }
                    e_req_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_req_id; text="req id"; type="editbox"; form=e_engagement_step_req_form; tooltip="The unique ID identifying the template requirement item. -- "; }
                    }
                e_engagement_step_req_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_req_name_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_req_name; text="req name"; type="editbox"; form=e_engagement_step_req_form; tooltip="A description for the engagement step requirement -- "; }
                    e_due_days_from_step_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_due_days_from_step; text="due days from step"; type="editbox"; form=e_engagement_step_req_form; tooltip="If applicable, how many days to set the due date at after the partner starts this step -- "; }
                    e_due_days_from_req_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_due_days_from_req; text="due days from req"; type="editbox"; form=e_engagement_step_req_form; tooltip="If applicable, how many days to set the due date at after the partner completes another requirement -- "; }
                    }
                e_engagement_step_req_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_due_days_from_req_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_due_days_from_req_id; text="due days from req id"; type="editbox"; form=e_engagement_step_req_form; tooltip="If applicable, the requirement ID whose completion triggers the due date interval for this requirement -- "; }
                    e_req_whom_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_req_whom; text="req whom"; type="editbox"; form=e_engagement_step_req_form; tooltip="Who this requirement is fulfilled by: (P)artner, (O)rganization, or (E)ither. -- "; }
                    e_req_doc_type_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_req_doc_type_id; text="req doc type id"; type="editbox"; form=e_engagement_step_req_form; tooltip="Does a document fulfill this step? If so, the corresponding document type ID is stored here. -- "; }
                    }
                e_engagement_step_req_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    e_req_waivable_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=e_req_waivable; text="req waivable"; type="editbox"; form=e_engagement_step_req_form; tooltip="Can this requirement be waived? 0 if not, or 1 if so -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=e_engagement_step_req_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=e_engagement_step_req_form; tooltip=" -- "; }
                    }
                e_engagement_step_req_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=e_engagement_step_req_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=e_engagement_step_req_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=e_engagement_step_req_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
