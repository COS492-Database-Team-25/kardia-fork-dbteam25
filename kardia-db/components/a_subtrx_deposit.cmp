$Version=2$
a_subtrx_deposit_component "widget/component-decl"
    {
    width=835;height=635;
    a_subtrx_deposit_osrc "widget/osrc"
        {
        sql="SELECT :a_amount, :a_num_checks, :a_batch_number, :a_effective_date, :a_period, :a_cash_amount, :__cx_osml_control, :a_comment, :a_ledger_number, :s_modified_by, :s_date_created, :a_account_code, :a_posted_to_gl, :s_date_modified, :a_posted, :s_created_by FROM /lightsys/partner_DB/a_subtrx_deposit/rows";
        baseobj = "/lightsys/partner_DB/a_subtrx_deposit/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        a_subtrx_deposit_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            a_subtrx_deposit_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            a_subtrx_deposit_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                a_subtrx_deposit_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_ledger_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_ledger_number; text="ledger number"; type="editbox"; form=a_subtrx_deposit_form; tooltip="ledger number for this deposit -- "; }
                    a_batch_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_batch_number; text="batch number"; type="editbox"; form=a_subtrx_deposit_form; tooltip="Batch id for this deposit -- "; }
                    a_period_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_period; text="period"; type="editbox"; form=a_subtrx_deposit_form; tooltip="Accounting period this deposit is recorded in. -- "; }
                    }
                a_subtrx_deposit_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_effective_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_effective_date; text="effective date"; type="editbox"; form=a_subtrx_deposit_form; tooltip="Effective date of deposit (e.g., accrual date) -- "; }
                    a_account_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_account_code; text="account code"; type="editbox"; form=a_subtrx_deposit_form; tooltip="Cash account the funds are deposited into -- "; }
                    a_amount_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_amount; text="amount"; type="editbox"; form=a_subtrx_deposit_form; tooltip="Total amount of deposit -- "; }
                    }
                a_subtrx_deposit_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_cash_amount_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_cash_amount; text="cash amount"; type="editbox"; form=a_subtrx_deposit_form; tooltip="Amount of cash deposited -- "; }
                    a_num_checks_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_num_checks; text="num checks"; type="editbox"; form=a_subtrx_deposit_form; tooltip="Number of checks deposited -- "; }
                    a_posted_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_posted; text="posted"; type="editbox"; form=a_subtrx_deposit_form; tooltip="Has this transfer been posted into the Deposits journal? -- "; }
                    }
                a_subtrx_deposit_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_posted_to_gl_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_posted_to_gl; text="posted to gl"; type="editbox"; form=a_subtrx_deposit_form; tooltip="Has this transfer been posted into the GL - yes (1) or no (0)? -- "; }
                    a_comment_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_comment; text="comment"; type="editbox"; form=a_subtrx_deposit_form; tooltip="Xfer comments -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=a_subtrx_deposit_form; tooltip=" -- "; }
                    }
                a_subtrx_deposit_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=a_subtrx_deposit_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=a_subtrx_deposit_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=a_subtrx_deposit_form; tooltip=" -- "; }
                    }
                a_subtrx_deposit_hbox5 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=a_subtrx_deposit_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
