$Version=2$
p_contact_usage_component "widget/component-decl"
    {
    width=835;height=635;
    p_contact_usage_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :p_end_date, :p_contact_or_location, :s_date_created, :p_contact_usage_type_code, :s_date_modified, :p_partner_key, :s_created_by, :__cx_osml_control, :p_start_date, :p_daterange_no_year, :p_contact_location_id FROM /lightsys/partner_DB/p_contact_usage/rows";
        baseobj = "/lightsys/partner_DB/p_contact_usage/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        p_contact_usage_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            p_contact_usage_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            p_contact_usage_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                p_contact_usage_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_contact_usage_type_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_contact_usage_type_code; text="contact usage type code"; type="editbox"; form=p_contact_usage_form; tooltip="Unique code for this usage type -- "; }
                    p_partner_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_partner_key; text="partner key"; type="editbox"; form=p_contact_usage_form; tooltip="The partner ID that this applies to -- "; }
                    p_contact_location_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_contact_location_id; text="contact location id"; type="editbox"; form=p_contact_usage_form; tooltip="The contact or location ID this applies to -- "; }
                    }
                p_contact_usage_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_contact_or_location_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_contact_or_location; text="contact or location"; type="editbox"; form=p_contact_usage_form; tooltip="C = contact, L = location -- "; }
                    p_start_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_start_date; text="start date"; type="editbox"; form=p_contact_usage_form; tooltip="The first validity date for this usage -- "; }
                    p_end_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_end_date; text="end date"; type="editbox"; form=p_contact_usage_form; tooltip="The last validity date for this usage -- "; }
                    }
                p_contact_usage_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_daterange_no_year_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_daterange_no_year; text="daterange no year"; type="editbox"; form=p_contact_usage_form; tooltip="If set to 1, ignore the year part of the start-end date range (the range recurs yearly) -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=p_contact_usage_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=p_contact_usage_form; tooltip=" -- "; }
                    }
                p_contact_usage_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=p_contact_usage_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=p_contact_usage_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=p_contact_usage_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
