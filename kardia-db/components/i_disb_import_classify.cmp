$Version=2$
i_disb_import_classify_component "widget/component-decl"
    {
    width=835;height=635;
    i_disb_import_classify_osrc "widget/osrc"
        {
        sql="SELECT :i_disb_insert, :i_disb_match_ckno, :__cx_osml_control, :a_comment, :i_disb_match_comment, :s_modified_by, :a_ledger_number, :i_disb_set_fund, :i_disb_match_payee, :i_disb_set_amount, :s_date_created, :i_disb_match_amount, :i_disb_match_year, :i_disb_classify_active, :i_disb_set_account, :s_date_modified, :s_created_by, :i_disb_classify_item FROM /lightsys/partner_DB/i_disb_import_classify/rows";
        baseobj = "/lightsys/partner_DB/i_disb_import_classify/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        i_disb_import_classify_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            i_disb_import_classify_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            i_disb_import_classify_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                i_disb_import_classify_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_ledger_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_ledger_number; text="ledger number"; type="editbox"; form=i_disb_import_classify_form; tooltip="ledger number for this classification rule -- "; }
                    i_disb_classify_item_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_classify_item; text="disb classify item"; type="editbox"; form=i_disb_import_classify_form; tooltip="unique id for this rule -- "; }
                    i_disb_classify_active_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_classify_active; text="disb classify active"; type="editbox"; form=i_disb_import_classify_form; tooltip="Whether this rule is active or not -- "; }
                    }
                i_disb_import_classify_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    i_disb_match_ckno_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_match_ckno; text="disb match ckno"; type="editbox"; form=i_disb_import_classify_form; tooltip="Match against check number -- "; }
                    i_disb_match_payee_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_match_payee; text="disb match payee"; type="editbox"; form=i_disb_import_classify_form; tooltip="Match against payee ID -- "; }
                    i_disb_match_year_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_match_year; text="disb match year"; type="editbox"; form=i_disb_import_classify_form; tooltip="Match against year -- "; }
                    }
                i_disb_import_classify_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    i_disb_match_amount_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_match_amount; text="disb match amount"; type="editbox"; form=i_disb_import_classify_form; tooltip="Match against amount -- "; }
                    i_disb_match_comment_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_match_comment; text="disb match comment"; type="editbox"; form=i_disb_import_classify_form; tooltip="Match against comments field (substring match) -- "; }
                    i_disb_insert_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_insert; text="disb insert"; type="editbox"; form=i_disb_import_classify_form; tooltip="Whether to modify the existing (0) or insert a new (1) disbursements record. For multi-line classifications, set the first to 0 and subsequent to 1. -- "; }
                    }
                i_disb_import_classify_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    i_disb_set_account_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_set_account; text="disb set account"; type="editbox"; form=i_disb_import_classify_form; tooltip="Set GL account for the expense / liability side of the transaction -- "; }
                    i_disb_set_fund_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_set_fund; text="disb set fund"; type="editbox"; form=i_disb_import_classify_form; tooltip="Set fund for the expense -- "; }
                    i_disb_set_amount_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=i_disb_set_amount; text="disb set amount"; type="editbox"; form=i_disb_import_classify_form; tooltip="Amount for this disbursement line item -- "; }
                    }
                i_disb_import_classify_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_comment_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_comment; text="comment"; type="editbox"; form=i_disb_import_classify_form; tooltip="A comment/description for this rule -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=i_disb_import_classify_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=i_disb_import_classify_form; tooltip=" -- "; }
                    }
                i_disb_import_classify_hbox5 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=i_disb_import_classify_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=i_disb_import_classify_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=i_disb_import_classify_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
