$Version=2$
s_global_search_component "widget/component-decl"
    {
    width=835;height=635;
    s_global_search_osrc "widget/osrc"
        {
        sql="SELECT :s_label, :s_desc, :__cx_osml_control, :s_search_res_id, :s_username, :s_cri5, :s_modified_by, :s_key, :s_search_id, :s_cri2, :s_cri3, :s_date_created, :s_cri1, :s_date_modified, :s_cri4, :s_created_by, :s_score, :s_type FROM /lightsys/partner_DB/s_global_search/rows";
        baseobj = "/lightsys/partner_DB/s_global_search/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        s_global_search_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            s_global_search_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            s_global_search_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                s_global_search_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_search_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_search_id; text="search id"; type="editbox"; form=s_global_search_form; tooltip="ID of search, a unique integer. -- "; }
                    s_username_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_username; text="username"; type="editbox"; form=s_global_search_form; tooltip="User who performed the search -- "; }
                    s_search_res_id_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_search_res_id; text="search res id"; type="editbox"; form=s_global_search_form; tooltip="ID of search result, a unique integer. -- "; }
                    }
                s_global_search_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_score_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_score; text="score"; type="editbox"; form=s_global_search_form; tooltip="Relevance of the result (0.0 = none, 100.0 = full) -- "; }
                    s_cri1_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_cri1; text="cri1"; type="editbox"; form=s_global_search_form; tooltip="Whether a given criteria was matched -- "; }
                    s_cri2_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_cri2; text="cri2"; type="editbox"; form=s_global_search_form; tooltip="Whether a given criteria was matched -- "; }
                    }
                s_global_search_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_cri3_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_cri3; text="cri3"; type="editbox"; form=s_global_search_form; tooltip="Whether a given criteria was matched -- "; }
                    s_cri4_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_cri4; text="cri4"; type="editbox"; form=s_global_search_form; tooltip="Whether a given criteria was matched -- "; }
                    s_cri5_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_cri5; text="cri5"; type="editbox"; form=s_global_search_form; tooltip="Whether a given criteria was matched -- "; }
                    }
                s_global_search_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_type_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_type; text="type"; type="editbox"; form=s_global_search_form; tooltip="Type of result (PAR = partner, etc.) -- "; }
                    s_label_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_label; text="label"; type="editbox"; form=s_global_search_form; tooltip="Brief label of search result -- "; }
                    s_desc_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_desc; text="desc"; type="editbox"; form=s_global_search_form; tooltip="Expanded description of search result -- "; }
                    }
                s_global_search_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_key; text="key"; type="editbox"; form=s_global_search_form; tooltip="Unique ID so we can find the actual relevant object for this result -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=s_global_search_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=s_global_search_form; tooltip=" -- "; }
                    }
                s_global_search_hbox5 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=s_global_search_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=s_global_search_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=s_global_search_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
