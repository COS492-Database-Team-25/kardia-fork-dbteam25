$Version=2$
a_cc_auto_subscribe_component "widget/component-decl"
    {
    width=835;height=635;
    a_cc_auto_subscribe_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :a_ledger_number, :m_list_code, :s_date_created, :s_date_modified, :a_comments, :a_cost_center, :s_created_by, :__cx_osml_control, :a_minimum_gift, :a_subscribe_months FROM /lightsys/partner_DB/a_cc_auto_subscribe/rows";
        baseobj = "/lightsys/partner_DB/a_cc_auto_subscribe/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        a_cc_auto_subscribe_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            a_cc_auto_subscribe_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            a_cc_auto_subscribe_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                a_cc_auto_subscribe_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_cost_center_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_cost_center; text="cost center"; type="editbox"; form=a_cc_auto_subscribe_form; tooltip="cost center code (alphanumeric allowed) -- "; }
                    a_ledger_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_ledger_number; text="ledger number"; type="editbox"; form=a_cc_auto_subscribe_form; tooltip="ledger number that uses this cost center -- "; }
                    m_list_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=m_list_code; text="list code"; type="editbox"; form=a_cc_auto_subscribe_form; tooltip="the mailing list code of the mailing to subscribe the donor to. -- "; }
                    }
                a_cc_auto_subscribe_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_minimum_gift_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_minimum_gift; text="minimum gift"; type="editbox"; form=a_cc_auto_subscribe_form; tooltip="sets a minimum gift before automatic subscription takes place. -- "; }
                    a_subscribe_months_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_subscribe_months; text="subscribe months"; type="editbox"; form=a_cc_auto_subscribe_form; tooltip="how many months to subscribe the donor (or indefinitely, if null) -- "; }
                    a_comments_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_comments; text="comments"; type="editbox"; form=a_cc_auto_subscribe_form; tooltip="comments -- "; }
                    }
                a_cc_auto_subscribe_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=a_cc_auto_subscribe_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=a_cc_auto_subscribe_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=a_cc_auto_subscribe_form; tooltip=" -- "; }
                    }
                a_cc_auto_subscribe_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=a_cc_auto_subscribe_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=a_cc_auto_subscribe_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
