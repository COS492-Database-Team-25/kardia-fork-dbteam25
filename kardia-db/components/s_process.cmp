$Version=2$
s_process_component "widget/component-decl"
    {
    width=835;height=635;
    s_process_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :s_date_created, :s_date_modified, :s_created_by, :s_process_desc, :__cx_osml_control, :s_process_code, :s_subsystem_code FROM /lightsys/partner_DB/s_process/rows";
        baseobj = "/lightsys/partner_DB/s_process/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        s_process_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            s_process_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            s_process_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                s_process_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_subsystem_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_subsystem_code; text="subsystem code"; type="editbox"; form=s_process_form; tooltip="Code for subsystem -- "; }
                    s_process_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_process_code; text="process code"; type="editbox"; form=s_process_form; tooltip="Code for process -- "; }
                    s_process_desc_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_process_desc; text="process desc"; type="editbox"; form=s_process_form; tooltip="description of process -- "; }
                    }
                s_process_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=s_process_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=s_process_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=s_process_form; tooltip=" -- "; }
                    }
                s_process_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=s_process_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=s_process_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
