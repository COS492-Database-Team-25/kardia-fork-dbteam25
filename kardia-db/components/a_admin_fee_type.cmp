$Version=2$
a_admin_fee_type_component "widget/component-decl"
    {
    width=835;height=635;
    a_admin_fee_type_osrc "widget/osrc"
        {
        sql="SELECT :s_modified_by, :a_ledger_number, :a_admin_fee_subtype, :a_is_default_subtype, :s_date_created, :a_default_percentage, :s_date_modified, :a_tmp_fixed_percentage, :s_created_by, :a_admin_fee_type_desc, :__cx_osml_control, :a_comment, :a_admin_fee_type, :a_tmp_total_percentage FROM /lightsys/partner_DB/a_admin_fee_type/rows";
        baseobj = "/lightsys/partner_DB/a_admin_fee_type/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        a_admin_fee_type_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            a_admin_fee_type_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            a_admin_fee_type_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                a_admin_fee_type_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_ledger_number_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_ledger_number; text="ledger number"; type="editbox"; form=a_admin_fee_type_form; tooltip="ledger number that uses this admin fee type -- "; }
                    a_admin_fee_type_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_admin_fee_type; text="admin fee type"; type="editbox"; form=a_admin_fee_type_form; tooltip="admin fee type -- "; }
                    a_admin_fee_subtype_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_admin_fee_subtype; text="admin fee subtype"; type="editbox"; form=a_admin_fee_type_form; tooltip="admin fee subtype - e.g., for variable percentages. -- "; }
                    }
                a_admin_fee_type_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_admin_fee_type_desc_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_admin_fee_type_desc; text="admin fee type desc"; type="editbox"; form=a_admin_fee_type_form; tooltip="description of this admin fee type -- "; }
                    a_is_default_subtype_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_is_default_subtype; text="is default subtype"; type="editbox"; form=a_admin_fee_type_form; tooltip="set to 1 to make this subtype the default one. -- "; }
                    a_default_percentage_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_default_percentage; text="default percentage"; type="editbox"; form=a_admin_fee_type_form; tooltip="percent of gift for entire admin fee type -- "; }
                    }
                a_admin_fee_type_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    a_tmp_total_percentage_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_tmp_total_percentage; text="tmp total percentage"; type="editbox"; form=a_admin_fee_type_form; tooltip="(tmp use) - total of item percentages -- "; }
                    a_tmp_fixed_percentage_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_tmp_fixed_percentage; text="tmp fixed percentage"; type="editbox"; form=a_admin_fee_type_form; tooltip="(tmp use) - total of "fixed" (non-proportional) item percentages -- "; }
                    a_comment_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=a_comment; text="comment"; type="editbox"; form=a_admin_fee_type_form; tooltip="comments for this admin fee type -- "; }
                    }
                a_admin_fee_type_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=a_admin_fee_type_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=a_admin_fee_type_form; tooltip=" -- "; }
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=a_admin_fee_type_form; tooltip=" -- "; }
                    }
                a_admin_fee_type_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=a_admin_fee_type_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=a_admin_fee_type_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
