$Version=2$
p_partner_component "widget/component-decl"
    {
    width=835;height=635;
    p_partner_osrc "widget/osrc"
        {
        sql="SELECT :p_title, :p_partner_class, :p_legacy_key_2, :p_legacy_key_3, :p_preferred_name, :p_record_status_code, :p_language_code, :p_status_change_date, :p_best_contact, :p_gender, :p_comments, :p_suffix, :p_no_mail, :s_date_created, :p_no_solicitations, :p_merged_with, :p_cost_center, :p_org_name, :p_surname, :p_no_mail_reason, :p_partner_key, :p_localized_name, :__cx_osml_control, :p_surname_first, :s_modified_by, :p_given_name, :p_legacy_key_1, :p_status_code, :p_acquisition_code, :s_date_modified, :s_created_by, :p_parent_key, :p_creating_office FROM /lightsys/partner_DB/p_partner/rows";
        baseobj = "/lightsys/partner_DB/p_partner/rows";
        replicasize=6;
        readahead=6;
        autoquery=onload;
        ConfirmWindow "widget/childwindow"
            {
            title = "Data Was Modified!";
            titlebar = yes;
            hdr_bgcolor="#c00000";
            bgcolor= "#e0e0e0";
            visible = false;
            style = dialog;
            x=200;y=200;width=300;height=140;

            warninglabel "widget/label"
                {
                x=10;y=10;width=276;height=30;
                text="Some data was modified.  Do you want to save it first, discard your modifications, or simply cancel the operation?";
                }

            _3bConfirmSave "widget/textbutton"
                {
                x=10;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Save";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmDiscard "widget/textbutton"
                {
                x=110;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Discard";
                fgcolor1=black;fgcolor2=white;
                }
            _3bConfirmCancel "widget/textbutton"
                {
                x=210;y=75;width=80;height=30;
                tristate=no;
                background="/sys/images/grey_gradient.png";
                text = "Cancel";
                fgcolor1=black;fgcolor2=white;
                }
            }
        p_partner_form "widget/form"
            {
            _3bconfirmwindow = "ConfirmWindow";
            p_partner_ctl "widget/component"
                {
                x=0;y=0;width=835;height=24;
                path="/sys/cmp/form_controls.cmp";
                bgcolor="#d0d0d0";
                }
            p_partner_vbox "widget/vbox"
                {
                x=10;y=40;width=800;height=800;cellsize=20; spacing=5;
                p_partner_hbox0 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_partner_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_partner_key; text="partner key"; type="editbox"; form=p_partner_form; tooltip=" -- "; }
                    p_creating_office_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_creating_office; text="creating office"; type="editbox"; form=p_partner_form; tooltip="partner key of creating office -- "; }
                    p_parent_key_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_parent_key; text="parent key"; type="editbox"; form=p_partner_form; tooltip="for partner hierarchies of various sorts -- "; }
                    }
                p_partner_hbox1 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_partner_class_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_partner_class; text="partner class"; type="editbox"; form=p_partner_form; tooltip=" -- "; }
                    p_status_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_status_code; text="status code"; type="editbox"; form=p_partner_form; tooltip="D=Deceased A=Active X=Removed by Constituent O=Obsolete for unknown reason -- "; }
                    p_status_change_date_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_status_change_date; text="status change date"; type="editbox"; form=p_partner_form; tooltip=" -- "; }
                    }
                p_partner_hbox2 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_title_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_title; text="title"; type="editbox"; form=p_partner_form; tooltip="Mr. & Mrs. -- "; }
                    p_given_name_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_given_name; text="given name"; type="editbox"; form=p_partner_form; tooltip="Jonathan and Jane -- "; }
                    p_preferred_name_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_preferred_name; text="preferred name"; type="editbox"; form=p_partner_form; tooltip="Jon and Jane -- "; }
                    }
                p_partner_hbox3 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_surname_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_surname; text="surname"; type="editbox"; form=p_partner_form; tooltip="Doe -- "; }
                    p_surname_first_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_surname_first; text="surname first"; type="editbox"; form=p_partner_form; tooltip="surname comes first, e.g., Korean -- "; }
                    p_localized_name_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_localized_name; text="localized name"; type="editbox"; form=p_partner_form; tooltip=" -- "; }
                    }
                p_partner_hbox4 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_suffix_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_suffix; text="suffix"; type="editbox"; form=p_partner_form; tooltip="such as "Jr.", "M.D.", "and Family", etc. -- "; }
                    p_org_name_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_org_name; text="org name"; type="editbox"; form=p_partner_form; tooltip=" -- "; }
                    p_gender_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_gender; text="gender"; type="editbox"; form=p_partner_form; tooltip="M=male, F=female, C=couple -- "; }
                    }
                p_partner_hbox5 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_language_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_language_code; text="language code"; type="editbox"; form=p_partner_form; tooltip=" -- "; }
                    p_acquisition_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_acquisition_code; text="acquisition code"; type="editbox"; form=p_partner_form; tooltip=" -- "; }
                    p_comments_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_comments; text="comments"; type="editbox"; form=p_partner_form; tooltip=" -- "; }
                    }
                p_partner_hbox6 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_record_status_code_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_record_status_code; text="record status code"; type="editbox"; form=p_partner_form; tooltip="A=active, O=obsolete, M=merged -- "; }
                    p_no_mail_reason_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_no_mail_reason; text="no mail reason"; type="editbox"; form=p_partner_form; tooltip="Reason why p_no_mail is set: (U)ndeliverable, (O)ffice Request, (P)ersonal Request, (M)issionary Request, (D)eceased, (T)emporarily Away, (I)nactive Donor, (X) Other. -- "; }
                    p_no_solicitations_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_no_solicitations; text="no solicitations"; type="editbox"; form=p_partner_form; tooltip=" -- "; }
                    }
                p_partner_hbox7 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_no_mail_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_no_mail; text="no mail"; type="editbox"; form=p_partner_form; tooltip=" -- "; }
                    p_cost_center_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_cost_center; text="cost center"; type="editbox"; form=p_partner_form; tooltip="The cost center number (see a_cost_center) associated with the partner, if applicable. -- "; }
                    p_best_contact_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_best_contact; text="best contact"; type="editbox"; form=p_partner_form; tooltip=" -- "; }
                    }
                p_partner_hbox8 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_merged_with_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_merged_with; text="merged with"; type="editbox"; form=p_partner_form; tooltip="new key id after merge -- "; }
                    p_legacy_key_1_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_legacy_key_1; text="legacy key 1"; type="editbox"; form=p_partner_form; tooltip="old system -- "; }
                    p_legacy_key_2_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_legacy_key_2; text="legacy key 2"; type="editbox"; form=p_partner_form; tooltip="old system -- "; }
                    }
                p_partner_hbox9 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    p_legacy_key_3_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=p_legacy_key_3; text="legacy key 3"; type="editbox"; form=p_partner_form; tooltip="old system -- "; }
                    s_date_created_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_created; text="date created"; type="editbox_moddate"; form=p_partner_form; tooltip=" -- "; }
                    s_created_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_created_by; text="created by"; type="editbox_creby"; form=p_partner_form; tooltip=" -- "; }
                    }
                p_partner_hbox10 "widget/hbox"
                {
                    x=0;y=0;
                    cellsize=250;
                    spacing=20;
                    width=750;
                    height=25;
                    s_date_modified_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_date_modified; text="date modified"; type="editbox_credate"; form=p_partner_form; tooltip=" -- "; }
                    s_modified_by_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=s_modified_by; text="modified by"; type="editbox_modby"; form=p_partner_form; tooltip=" -- "; }
                    __cx_osml_control_cmp "widget/component" { path="/lightsys/smart_field.cmp"; field=__cx_osml_control; text=" cx osml control"; type="editbox"; form=p_partner_form; tooltip=" -- "; }
                    }
                }
            }
        }
    }
